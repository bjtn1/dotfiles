{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c5cc",
                "text": "#c4c5cc",
                "text.muted": "#c4c5cc70",
                "text.ignored": "#c4c5cc40",
                "text.placeholder": "#c4c5cc50",
                "ignored": "#c4c5cc30",
                "element.hover": "#c4c5cc30",
                "ghost_element.hover": "#4A5BAB30",
                "ghost_element.selected": "#4A5BAB30",
                "ghost_element.active": "#4A5BAB60",
                "border": "#000000",
                "editor.highlighted_line.background": "#4A5BAB10",
                "editor.active_line.background": "#4A5BAB10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#4A5BAB30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#4A5BAB",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c5cc",
                "terminal.dim_foreground": "#c4c5cc",
                "terminal.bright_foreground": "#c4c5cc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#4A5BAB",
                "terminal.ansi.green": "#5677C7",
                "terminal.ansi.yellow": "#8579C4",
                "terminal.ansi.blue": "#6894DE",
                "terminal.ansi.magenta": "#77C2F9",
                "terminal.ansi.cyan": "#98ABEE",
                "terminal.ansi.white": "#c4c5cc",
                "terminal.ansi.bright_black": "#65677c",
                "terminal.ansi.bright_red": "#4A5BAB",
                "terminal.ansi.bright_green": "#5677C7",
                "terminal.ansi.bright_yellow": "#8579C4",
                "terminal.ansi.bright_blue": "#6894DE",
                "terminal.ansi.bright_magenta": "#77C2F9",
                "terminal.ansi.bright_cyan": "#98ABEE",
                "terminal.ansi.bright_white": "#c4c5cc",
                "modified": "#4A5BAB",
                "syntax": {
                    "attribute": {
                        "color": "#4A5BAB"
                    },
                    "boolean": {
                      "color": "#5677C7"
                    },
                    "comment": {
                      "color": "#65677c"
                    },
                    "comment.doc": {
                      "color": "#65677c"
                    },
                    "constant": {
                      "color": "#5677C7"
                    },
                    "function": {
                      "color": "#98ABEE"
                    },
                    "keyword": {
                      "color": "#6894DE"
                    },
                    "number": {
                      "color": "#77C2F9"
                    },
                    "operator": {
                      "color": "#6894DE"
                    },
                    "string": {
                      "color": "#4A5BAB"
                    },
                    "variable": {
                      "color": "#5677C7"
                    }
                },
                "players": [
                  {
                    "cursor": "#4A5BAB",
                    "background": "#000000",
                    "selection": "#4A5BAB30"
                  }
                ]
            }
        }
    ]
}
