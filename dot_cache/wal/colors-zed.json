{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c9c9",
                "text": "#c3c9c9",
                "text.muted": "#c3c9c970",
                "text.ignored": "#c3c9c940",
                "text.placeholder": "#c3c9c950",
                "ignored": "#c3c9c930",
                "element.hover": "#c3c9c930",
                "ghost_element.hover": "#BDCA5130",
                "ghost_element.selected": "#BDCA5130",
                "ghost_element.active": "#BDCA5160",
                "border": "#000000",
                "editor.highlighted_line.background": "#BDCA5110",
                "editor.active_line.background": "#BDCA5110",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#BDCA5130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#BDCA51",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c9c9",
                "terminal.dim_foreground": "#c3c9c9",
                "terminal.bright_foreground": "#c3c9c9",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#BDCA51",
                "terminal.ansi.green": "#F3E76C",
                "terminal.ansi.yellow": "#096B95",
                "terminal.ansi.blue": "#1D949C",
                "terminal.ansi.magenta": "#23B0DE",
                "terminal.ansi.cyan": "#4DC3CF",
                "terminal.ansi.white": "#c3c9c9",
                "terminal.ansi.bright_black": "#607676",
                "terminal.ansi.bright_red": "#BDCA51",
                "terminal.ansi.bright_green": "#F3E76C",
                "terminal.ansi.bright_yellow": "#096B95",
                "terminal.ansi.bright_blue": "#1D949C",
                "terminal.ansi.bright_magenta": "#23B0DE",
                "terminal.ansi.bright_cyan": "#4DC3CF",
                "terminal.ansi.bright_white": "#c3c9c9",
                "modified": "#BDCA51",
                "syntax": {
                    "attribute": {
                        "color": "#BDCA51"
                    },
                    "boolean": {
                      "color": "#F3E76C"
                    },
                    "comment": {
                      "color": "#607676"
                    },
                    "comment.doc": {
                      "color": "#607676"
                    },
                    "constant": {
                      "color": "#F3E76C"
                    },
                    "function": {
                      "color": "#4DC3CF"
                    },
                    "keyword": {
                      "color": "#1D949C"
                    },
                    "number": {
                      "color": "#23B0DE"
                    },
                    "operator": {
                      "color": "#1D949C"
                    },
                    "string": {
                      "color": "#BDCA51"
                    },
                    "variable": {
                      "color": "#F3E76C"
                    }
                },
                "players": [
                  {
                    "cursor": "#BDCA51",
                    "background": "#000000",
                    "selection": "#BDCA5130"
                  }
                ]
            }
        }
    ]
}
