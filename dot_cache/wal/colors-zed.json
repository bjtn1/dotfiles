{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c5cc",
                "text": "#c4c5cc",
                "text.muted": "#c4c5cc70",
                "text.ignored": "#c4c5cc40",
                "text.placeholder": "#c4c5cc50",
                "ignored": "#c4c5cc30",
                "element.hover": "#c4c5cc30",
                "ghost_element.hover": "#BFAA3B30",
                "ghost_element.selected": "#BFAA3B30",
                "ghost_element.active": "#BFAA3B60",
                "border": "#000000",
                "editor.highlighted_line.background": "#BFAA3B10",
                "editor.active_line.background": "#BFAA3B10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#BFAA3B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#BFAA3B",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c5cc",
                "terminal.dim_foreground": "#c4c5cc",
                "terminal.bright_foreground": "#c4c5cc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#BFAA3B",
                "terminal.ansi.green": "#DFA923",
                "terminal.ansi.yellow": "#2451AF",
                "terminal.ansi.blue": "#57689B",
                "terminal.ansi.magenta": "#5E9FD5",
                "terminal.ansi.cyan": "#44ADCA",
                "terminal.ansi.white": "#c4c5cc",
                "terminal.ansi.bright_black": "#65667b",
                "terminal.ansi.bright_red": "#BFAA3B",
                "terminal.ansi.bright_green": "#DFA923",
                "terminal.ansi.bright_yellow": "#2451AF",
                "terminal.ansi.bright_blue": "#57689B",
                "terminal.ansi.bright_magenta": "#5E9FD5",
                "terminal.ansi.bright_cyan": "#44ADCA",
                "terminal.ansi.bright_white": "#c4c5cc",
                "modified": "#BFAA3B",
                "syntax": {
                    "attribute": {
                        "color": "#BFAA3B"
                    },
                    "boolean": {
                      "color": "#DFA923"
                    },
                    "comment": {
                      "color": "#65667b"
                    },
                    "comment.doc": {
                      "color": "#65667b"
                    },
                    "constant": {
                      "color": "#DFA923"
                    },
                    "function": {
                      "color": "#44ADCA"
                    },
                    "keyword": {
                      "color": "#57689B"
                    },
                    "number": {
                      "color": "#5E9FD5"
                    },
                    "operator": {
                      "color": "#57689B"
                    },
                    "string": {
                      "color": "#BFAA3B"
                    },
                    "variable": {
                      "color": "#DFA923"
                    }
                },
                "players": [
                  {
                    "cursor": "#BFAA3B",
                    "background": "#000000",
                    "selection": "#BFAA3B30"
                  }
                ]
            }
        }
    ]
}
