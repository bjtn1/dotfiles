{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c7cccd",
                "text": "#c7cccd",
                "text.muted": "#c7cccd70",
                "text.ignored": "#c7cccd40",
                "text.placeholder": "#c7cccd50",
                "ignored": "#c7cccd30",
                "element.hover": "#c7cccd30",
                "ghost_element.hover": "#A891B530",
                "ghost_element.selected": "#A891B530",
                "ghost_element.active": "#A891B560",
                "border": "#000000",
                "editor.highlighted_line.background": "#A891B510",
                "editor.active_line.background": "#A891B510",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#A891B530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#A891B5",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c7cccd",
                "terminal.dim_foreground": "#c7cccd",
                "terminal.bright_foreground": "#c7cccd",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#A891B5",
                "terminal.ansi.green": "#D5B6AB",
                "terminal.ansi.yellow": "#A1AED3",
                "terminal.ansi.blue": "#DAAFCC",
                "terminal.ansi.magenta": "#B1CEE4",
                "terminal.ansi.cyan": "#A0D3E6",
                "terminal.ansi.white": "#c7cccd",
                "terminal.ansi.bright_black": "#697c81",
                "terminal.ansi.bright_red": "#A891B5",
                "terminal.ansi.bright_green": "#D5B6AB",
                "terminal.ansi.bright_yellow": "#A1AED3",
                "terminal.ansi.bright_blue": "#DAAFCC",
                "terminal.ansi.bright_magenta": "#B1CEE4",
                "terminal.ansi.bright_cyan": "#A0D3E6",
                "terminal.ansi.bright_white": "#c7cccd",
                "modified": "#A891B5",
                "syntax": {
                    "attribute": {
                        "color": "#A891B5"
                    },
                    "boolean": {
                      "color": "#D5B6AB"
                    },
                    "comment": {
                      "color": "#697c81"
                    },
                    "comment.doc": {
                      "color": "#697c81"
                    },
                    "constant": {
                      "color": "#D5B6AB"
                    },
                    "function": {
                      "color": "#A0D3E6"
                    },
                    "keyword": {
                      "color": "#DAAFCC"
                    },
                    "number": {
                      "color": "#B1CEE4"
                    },
                    "operator": {
                      "color": "#DAAFCC"
                    },
                    "string": {
                      "color": "#A891B5"
                    },
                    "variable": {
                      "color": "#D5B6AB"
                    }
                },
                "players": [
                  {
                    "cursor": "#A891B5",
                    "background": "#000000",
                    "selection": "#A891B530"
                  }
                ]
            }
        }
    ]
}
