{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6c7c2",
                "text": "#c6c7c2",
                "text.muted": "#c6c7c270",
                "text.ignored": "#c6c7c240",
                "text.placeholder": "#c6c7c250",
                "ignored": "#c6c7c230",
                "element.hover": "#c6c7c230",
                "ghost_element.hover": "#69605330",
                "ghost_element.selected": "#69605330",
                "ghost_element.active": "#69605360",
                "border": "#000000",
                "editor.highlighted_line.background": "#69605310",
                "editor.active_line.background": "#69605310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#69605330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#696053",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6c7c2",
                "terminal.dim_foreground": "#c6c7c2",
                "terminal.bright_foreground": "#c6c7c2",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#696053",
                "terminal.ansi.green": "#915C39",
                "terminal.ansi.yellow": "#8E6C56",
                "terminal.ansi.blue": "#A2926B",
                "terminal.ansi.magenta": "#D0AB6B",
                "terminal.ansi.cyan": "#5C7583",
                "terminal.ansi.white": "#c6c7c2",
                "terminal.ansi.bright_black": "#6b715d",
                "terminal.ansi.bright_red": "#696053",
                "terminal.ansi.bright_green": "#915C39",
                "terminal.ansi.bright_yellow": "#8E6C56",
                "terminal.ansi.bright_blue": "#A2926B",
                "terminal.ansi.bright_magenta": "#D0AB6B",
                "terminal.ansi.bright_cyan": "#5C7583",
                "terminal.ansi.bright_white": "#c6c7c2",
                "modified": "#696053",
                "syntax": {
                    "attribute": {
                        "color": "#696053"
                    },
                    "boolean": {
                      "color": "#915C39"
                    },
                    "comment": {
                      "color": "#6b715d"
                    },
                    "comment.doc": {
                      "color": "#6b715d"
                    },
                    "constant": {
                      "color": "#915C39"
                    },
                    "function": {
                      "color": "#5C7583"
                    },
                    "keyword": {
                      "color": "#A2926B"
                    },
                    "number": {
                      "color": "#D0AB6B"
                    },
                    "operator": {
                      "color": "#A2926B"
                    },
                    "string": {
                      "color": "#696053"
                    },
                    "variable": {
                      "color": "#915C39"
                    }
                },
                "players": [
                  {
                    "cursor": "#696053",
                    "background": "#000000",
                    "selection": "#69605330"
                  }
                ]
            }
        }
    ]
}
