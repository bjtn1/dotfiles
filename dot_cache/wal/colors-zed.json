{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c7cbcc",
                "text": "#c7cbcc",
                "text.muted": "#c7cbcc70",
                "text.ignored": "#c7cbcc40",
                "text.placeholder": "#c7cbcc50",
                "ignored": "#c7cbcc30",
                "element.hover": "#c7cbcc30",
                "ghost_element.hover": "#68CAFC30",
                "ghost_element.selected": "#68CAFC30",
                "ghost_element.active": "#68CAFC60",
                "border": "#000000",
                "editor.highlighted_line.background": "#68CAFC10",
                "editor.active_line.background": "#68CAFC10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#68CAFC30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#68CAFC",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c7cbcc",
                "terminal.dim_foreground": "#c7cbcc",
                "terminal.bright_foreground": "#c7cbcc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#68CAFC",
                "terminal.ansi.green": "#B3ADA8",
                "terminal.ansi.yellow": "#F3A0A5",
                "terminal.ansi.blue": "#E7D2A6",
                "terminal.ansi.magenta": "#A1DBF9",
                "terminal.ansi.cyan": "#EFDCD6",
                "terminal.ansi.white": "#c7cbcc",
                "terminal.ansi.bright_black": "#687d7f",
                "terminal.ansi.bright_red": "#68CAFC",
                "terminal.ansi.bright_green": "#B3ADA8",
                "terminal.ansi.bright_yellow": "#F3A0A5",
                "terminal.ansi.bright_blue": "#E7D2A6",
                "terminal.ansi.bright_magenta": "#A1DBF9",
                "terminal.ansi.bright_cyan": "#EFDCD6",
                "terminal.ansi.bright_white": "#c7cbcc",
                "modified": "#68CAFC",
                "syntax": {
                    "attribute": {
                        "color": "#68CAFC"
                    },
                    "boolean": {
                      "color": "#B3ADA8"
                    },
                    "comment": {
                      "color": "#687d7f"
                    },
                    "comment.doc": {
                      "color": "#687d7f"
                    },
                    "constant": {
                      "color": "#B3ADA8"
                    },
                    "function": {
                      "color": "#EFDCD6"
                    },
                    "keyword": {
                      "color": "#E7D2A6"
                    },
                    "number": {
                      "color": "#A1DBF9"
                    },
                    "operator": {
                      "color": "#E7D2A6"
                    },
                    "string": {
                      "color": "#68CAFC"
                    },
                    "variable": {
                      "color": "#B3ADA8"
                    }
                },
                "players": [
                  {
                    "cursor": "#68CAFC",
                    "background": "#000000",
                    "selection": "#68CAFC30"
                  }
                ]
            }
        }
    ]
}
