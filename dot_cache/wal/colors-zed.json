{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c2c6",
                "text": "#c3c2c6",
                "text.muted": "#c3c2c670",
                "text.ignored": "#c3c2c640",
                "text.placeholder": "#c3c2c650",
                "ignored": "#c3c2c630",
                "element.hover": "#c3c2c630",
                "ghost_element.hover": "#3267DE30",
                "ghost_element.selected": "#3267DE30",
                "ghost_element.active": "#3267DE60",
                "border": "#000000",
                "editor.highlighted_line.background": "#3267DE10",
                "editor.active_line.background": "#3267DE10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#3267DE30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#3267DE",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c2c6",
                "terminal.dim_foreground": "#c3c2c6",
                "terminal.bright_foreground": "#c3c2c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#3267DE",
                "terminal.ansi.green": "#5B6ADA",
                "terminal.ansi.yellow": "#9966C1",
                "terminal.ansi.blue": "#418AEF",
                "terminal.ansi.magenta": "#6192F4",
                "terminal.ansi.cyan": "#9A9CF7",
                "terminal.ansi.white": "#c3c2c6",
                "terminal.ansi.bright_black": "#5f5b70",
                "terminal.ansi.bright_red": "#3267DE",
                "terminal.ansi.bright_green": "#5B6ADA",
                "terminal.ansi.bright_yellow": "#9966C1",
                "terminal.ansi.bright_blue": "#418AEF",
                "terminal.ansi.bright_magenta": "#6192F4",
                "terminal.ansi.bright_cyan": "#9A9CF7",
                "terminal.ansi.bright_white": "#c3c2c6",
                "modified": "#3267DE",
                "syntax": {
                    "attribute": {
                        "color": "#3267DE"
                    },
                    "boolean": {
                      "color": "#5B6ADA"
                    },
                    "comment": {
                      "color": "#5f5b70"
                    },
                    "comment.doc": {
                      "color": "#5f5b70"
                    },
                    "constant": {
                      "color": "#5B6ADA"
                    },
                    "function": {
                      "color": "#9A9CF7"
                    },
                    "keyword": {
                      "color": "#418AEF"
                    },
                    "number": {
                      "color": "#6192F4"
                    },
                    "operator": {
                      "color": "#418AEF"
                    },
                    "string": {
                      "color": "#3267DE"
                    },
                    "variable": {
                      "color": "#5B6ADA"
                    }
                },
                "players": [
                  {
                    "cursor": "#3267DE",
                    "background": "#000000",
                    "selection": "#3267DE30"
                  }
                ]
            }
        }
    ]
}
