{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c1c4",
                "text": "#c2c1c4",
                "text.muted": "#c2c1c470",
                "text.ignored": "#c2c1c440",
                "text.placeholder": "#c2c1c450",
                "ignored": "#c2c1c430",
                "element.hover": "#c2c1c430",
                "ghost_element.hover": "#D23B3A30",
                "ghost_element.selected": "#D23B3A30",
                "ghost_element.active": "#D23B3A60",
                "border": "#000000",
                "editor.highlighted_line.background": "#D23B3A10",
                "editor.active_line.background": "#D23B3A10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#D23B3A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#D23B3A",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c1c4",
                "terminal.dim_foreground": "#c2c1c4",
                "terminal.bright_foreground": "#c2c1c4",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#D23B3A",
                "terminal.ansi.green": "#EA5731",
                "terminal.ansi.yellow": "#A0354D",
                "terminal.ansi.blue": "#C35A55",
                "terminal.ansi.magenta": "#F99833",
                "terminal.ansi.cyan": "#FDC839",
                "terminal.ansi.white": "#c2c1c4",
                "terminal.ansi.bright_black": "#5b596c",
                "terminal.ansi.bright_red": "#D23B3A",
                "terminal.ansi.bright_green": "#EA5731",
                "terminal.ansi.bright_yellow": "#A0354D",
                "terminal.ansi.bright_blue": "#C35A55",
                "terminal.ansi.bright_magenta": "#F99833",
                "terminal.ansi.bright_cyan": "#FDC839",
                "terminal.ansi.bright_white": "#c2c1c4",
                "modified": "#D23B3A",
                "syntax": {
                    "attribute": {
                        "color": "#D23B3A"
                    },
                    "boolean": {
                      "color": "#EA5731"
                    },
                    "comment": {
                      "color": "#5b596c"
                    },
                    "comment.doc": {
                      "color": "#5b596c"
                    },
                    "constant": {
                      "color": "#EA5731"
                    },
                    "function": {
                      "color": "#FDC839"
                    },
                    "keyword": {
                      "color": "#C35A55"
                    },
                    "number": {
                      "color": "#F99833"
                    },
                    "operator": {
                      "color": "#C35A55"
                    },
                    "string": {
                      "color": "#D23B3A"
                    },
                    "variable": {
                      "color": "#EA5731"
                    }
                },
                "players": [
                  {
                    "cursor": "#D23B3A",
                    "background": "#000000",
                    "selection": "#D23B3A30"
                  }
                ]
            }
        }
    ]
}
