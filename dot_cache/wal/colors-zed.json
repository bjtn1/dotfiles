{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#D65F3930",
                "ghost_element.selected": "#D65F3930",
                "ghost_element.active": "#D65F3960",
                "border": "#000000",
                "editor.highlighted_line.background": "#D65F3910",
                "editor.active_line.background": "#D65F3910",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#D65F3930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#D65F39",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#D65F39",
                "terminal.ansi.green": "#9D6A4F",
                "terminal.ansi.yellow": "#C89548",
                "terminal.ansi.blue": "#DFA05A",
                "terminal.ansi.magenta": "#FADD70",
                "terminal.ansi.cyan": "#668EAB",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#59596d",
                "terminal.ansi.bright_red": "#D65F39",
                "terminal.ansi.bright_green": "#9D6A4F",
                "terminal.ansi.bright_yellow": "#C89548",
                "terminal.ansi.bright_blue": "#DFA05A",
                "terminal.ansi.bright_magenta": "#FADD70",
                "terminal.ansi.bright_cyan": "#668EAB",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#D65F39",
                "syntax": {
                    "attribute": {
                        "color": "#D65F39"
                    },
                    "boolean": {
                      "color": "#9D6A4F"
                    },
                    "comment": {
                      "color": "#59596d"
                    },
                    "comment.doc": {
                      "color": "#59596d"
                    },
                    "constant": {
                      "color": "#9D6A4F"
                    },
                    "function": {
                      "color": "#668EAB"
                    },
                    "keyword": {
                      "color": "#DFA05A"
                    },
                    "number": {
                      "color": "#FADD70"
                    },
                    "operator": {
                      "color": "#DFA05A"
                    },
                    "string": {
                      "color": "#D65F39"
                    },
                    "variable": {
                      "color": "#9D6A4F"
                    }
                },
                "players": [
                  {
                    "cursor": "#D65F39",
                    "background": "#000000",
                    "selection": "#D65F3930"
                  }
                ]
            }
        }
    ]
}
