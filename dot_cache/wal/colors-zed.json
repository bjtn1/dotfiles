{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c7c2c7",
                "text": "#c7c2c7",
                "text.muted": "#c7c2c770",
                "text.ignored": "#c7c2c740",
                "text.placeholder": "#c7c2c750",
                "ignored": "#c7c2c730",
                "element.hover": "#c7c2c730",
                "ghost_element.hover": "#C4917130",
                "ghost_element.selected": "#C4917130",
                "ghost_element.active": "#C4917160",
                "border": "#000000",
                "editor.highlighted_line.background": "#C4917110",
                "editor.active_line.background": "#C4917110",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#C4917130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#C49171",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c7c2c7",
                "terminal.dim_foreground": "#c7c2c7",
                "terminal.bright_foreground": "#c7c2c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#C49171",
                "terminal.ansi.green": "#36548D",
                "terminal.ansi.yellow": "#636C90",
                "terminal.ansi.blue": "#9A7887",
                "terminal.ansi.magenta": "#C17F82",
                "terminal.ansi.cyan": "#6F92B2",
                "terminal.ansi.white": "#c7c2c7",
                "terminal.ansi.bright_black": "#715d71",
                "terminal.ansi.bright_red": "#C49171",
                "terminal.ansi.bright_green": "#36548D",
                "terminal.ansi.bright_yellow": "#636C90",
                "terminal.ansi.bright_blue": "#9A7887",
                "terminal.ansi.bright_magenta": "#C17F82",
                "terminal.ansi.bright_cyan": "#6F92B2",
                "terminal.ansi.bright_white": "#c7c2c7",
                "modified": "#C49171",
                "syntax": {
                    "attribute": {
                        "color": "#C49171"
                    },
                    "boolean": {
                      "color": "#36548D"
                    },
                    "comment": {
                      "color": "#715d71"
                    },
                    "comment.doc": {
                      "color": "#715d71"
                    },
                    "constant": {
                      "color": "#36548D"
                    },
                    "function": {
                      "color": "#6F92B2"
                    },
                    "keyword": {
                      "color": "#9A7887"
                    },
                    "number": {
                      "color": "#C17F82"
                    },
                    "operator": {
                      "color": "#9A7887"
                    },
                    "string": {
                      "color": "#C49171"
                    },
                    "variable": {
                      "color": "#36548D"
                    }
                },
                "players": [
                  {
                    "cursor": "#C49171",
                    "background": "#000000",
                    "selection": "#C4917130"
                  }
                ]
            }
        }
    ]
}
