{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c7c4",
                "text": "#c2c7c4",
                "text.muted": "#c2c7c470",
                "text.ignored": "#c2c7c440",
                "text.placeholder": "#c2c7c450",
                "ignored": "#c2c7c430",
                "element.hover": "#c2c7c430",
                "ghost_element.hover": "#9EB85C30",
                "ghost_element.selected": "#9EB85C30",
                "ghost_element.active": "#9EB85C60",
                "border": "#000000",
                "editor.highlighted_line.background": "#9EB85C10",
                "editor.active_line.background": "#9EB85C10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#9EB85C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#9EB85C",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c7c4",
                "terminal.dim_foreground": "#c2c7c4",
                "terminal.bright_foreground": "#c2c7c4",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#9EB85C",
                "terminal.ansi.green": "#57748E",
                "terminal.ansi.yellow": "#6F8D9F",
                "terminal.ansi.blue": "#35B7FC",
                "terminal.ansi.magenta": "#5BB5F6",
                "terminal.ansi.cyan": "#6DC0F6",
                "terminal.ansi.white": "#c2c7c4",
                "terminal.ansi.bright_black": "#5c7165",
                "terminal.ansi.bright_red": "#9EB85C",
                "terminal.ansi.bright_green": "#57748E",
                "terminal.ansi.bright_yellow": "#6F8D9F",
                "terminal.ansi.bright_blue": "#35B7FC",
                "terminal.ansi.bright_magenta": "#5BB5F6",
                "terminal.ansi.bright_cyan": "#6DC0F6",
                "terminal.ansi.bright_white": "#c2c7c4",
                "modified": "#9EB85C",
                "syntax": {
                    "attribute": {
                        "color": "#9EB85C"
                    },
                    "boolean": {
                      "color": "#57748E"
                    },
                    "comment": {
                      "color": "#5c7165"
                    },
                    "comment.doc": {
                      "color": "#5c7165"
                    },
                    "constant": {
                      "color": "#57748E"
                    },
                    "function": {
                      "color": "#6DC0F6"
                    },
                    "keyword": {
                      "color": "#35B7FC"
                    },
                    "number": {
                      "color": "#5BB5F6"
                    },
                    "operator": {
                      "color": "#35B7FC"
                    },
                    "string": {
                      "color": "#9EB85C"
                    },
                    "variable": {
                      "color": "#57748E"
                    }
                },
                "players": [
                  {
                    "cursor": "#9EB85C",
                    "background": "#000000",
                    "selection": "#9EB85C30"
                  }
                ]
            }
        }
    ]
}
