{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6c3c2",
                "text": "#c6c3c2",
                "text.muted": "#c6c3c270",
                "text.ignored": "#c6c3c240",
                "text.placeholder": "#c6c3c250",
                "ignored": "#c6c3c230",
                "element.hover": "#c6c3c230",
                "ghost_element.hover": "#D9682930",
                "ghost_element.selected": "#D9682930",
                "ghost_element.active": "#D9682960",
                "border": "#000000",
                "editor.highlighted_line.background": "#D9682910",
                "editor.active_line.background": "#D9682910",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#D9682930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#D96829",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6c3c2",
                "terminal.dim_foreground": "#c6c3c2",
                "terminal.bright_foreground": "#c6c3c2",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#D96829",
                "terminal.ansi.green": "#AB7251",
                "terminal.ansi.yellow": "#DB9B58",
                "terminal.ansi.blue": "#EBB65B",
                "terminal.ansi.magenta": "#987F80",
                "terminal.ansi.cyan": "#C7A99F",
                "terminal.ansi.white": "#c6c3c2",
                "terminal.ansi.bright_black": "#70615b",
                "terminal.ansi.bright_red": "#D96829",
                "terminal.ansi.bright_green": "#AB7251",
                "terminal.ansi.bright_yellow": "#DB9B58",
                "terminal.ansi.bright_blue": "#EBB65B",
                "terminal.ansi.bright_magenta": "#987F80",
                "terminal.ansi.bright_cyan": "#C7A99F",
                "terminal.ansi.bright_white": "#c6c3c2",
                "modified": "#D96829",
                "syntax": {
                    "attribute": {
                        "color": "#D96829"
                    },
                    "boolean": {
                      "color": "#AB7251"
                    },
                    "comment": {
                      "color": "#70615b"
                    },
                    "comment.doc": {
                      "color": "#70615b"
                    },
                    "constant": {
                      "color": "#AB7251"
                    },
                    "function": {
                      "color": "#C7A99F"
                    },
                    "keyword": {
                      "color": "#EBB65B"
                    },
                    "number": {
                      "color": "#987F80"
                    },
                    "operator": {
                      "color": "#EBB65B"
                    },
                    "string": {
                      "color": "#D96829"
                    },
                    "variable": {
                      "color": "#AB7251"
                    }
                },
                "players": [
                  {
                    "cursor": "#D96829",
                    "background": "#000000",
                    "selection": "#D9682930"
                  }
                ]
            }
        }
    ]
}
