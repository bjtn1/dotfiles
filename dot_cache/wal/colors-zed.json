{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c5ca",
                "text": "#c4c5ca",
                "text.muted": "#c4c5ca70",
                "text.ignored": "#c4c5ca40",
                "text.placeholder": "#c4c5ca50",
                "ignored": "#c4c5ca30",
                "element.hover": "#c4c5ca30",
                "ghost_element.hover": "#556E9C30",
                "ghost_element.selected": "#556E9C30",
                "ghost_element.active": "#556E9C60",
                "border": "#000000",
                "editor.highlighted_line.background": "#556E9C10",
                "editor.active_line.background": "#556E9C10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#556E9C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#556E9C",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c5ca",
                "terminal.dim_foreground": "#c4c5ca",
                "terminal.bright_foreground": "#c4c5ca",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#556E9C",
                "terminal.ansi.green": "#937989",
                "terminal.ansi.yellow": "#F5628D",
                "terminal.ansi.blue": "#459FD0",
                "terminal.ansi.magenta": "#9D8896",
                "terminal.ansi.cyan": "#DCABA9",
                "terminal.ansi.white": "#c4c5ca",
                "terminal.ansi.bright_black": "#636679",
                "terminal.ansi.bright_red": "#556E9C",
                "terminal.ansi.bright_green": "#937989",
                "terminal.ansi.bright_yellow": "#F5628D",
                "terminal.ansi.bright_blue": "#459FD0",
                "terminal.ansi.bright_magenta": "#9D8896",
                "terminal.ansi.bright_cyan": "#DCABA9",
                "terminal.ansi.bright_white": "#c4c5ca",
                "modified": "#556E9C",
                "syntax": {
                    "attribute": {
                        "color": "#556E9C"
                    },
                    "boolean": {
                      "color": "#937989"
                    },
                    "comment": {
                      "color": "#636679"
                    },
                    "comment.doc": {
                      "color": "#636679"
                    },
                    "constant": {
                      "color": "#937989"
                    },
                    "function": {
                      "color": "#DCABA9"
                    },
                    "keyword": {
                      "color": "#459FD0"
                    },
                    "number": {
                      "color": "#9D8896"
                    },
                    "operator": {
                      "color": "#459FD0"
                    },
                    "string": {
                      "color": "#556E9C"
                    },
                    "variable": {
                      "color": "#937989"
                    }
                },
                "players": [
                  {
                    "cursor": "#556E9C",
                    "background": "#000000",
                    "selection": "#556E9C30"
                  }
                ]
            }
        }
    ]
}
