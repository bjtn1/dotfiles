{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c3c6",
                "text": "#c4c3c6",
                "text.muted": "#c4c3c670",
                "text.ignored": "#c4c3c640",
                "text.placeholder": "#c4c3c650",
                "ignored": "#c4c3c630",
                "element.hover": "#c4c3c630",
                "ghost_element.hover": "#F05B9A30",
                "ghost_element.selected": "#F05B9A30",
                "ghost_element.active": "#F05B9A60",
                "border": "#000000",
                "editor.highlighted_line.background": "#F05B9A10",
                "editor.active_line.background": "#F05B9A10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#F05B9A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#F05B9A",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c3c6",
                "terminal.dim_foreground": "#c4c3c6",
                "terminal.bright_foreground": "#c4c3c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#F05B9A",
                "terminal.ansi.green": "#F730F5",
                "terminal.ansi.yellow": "#F461ED",
                "terminal.ansi.blue": "#69A6F3",
                "terminal.ansi.magenta": "#FC9F99",
                "terminal.ansi.cyan": "#B4B8E0",
                "terminal.ansi.white": "#c4c3c6",
                "terminal.ansi.bright_black": "#605d71",
                "terminal.ansi.bright_red": "#F05B9A",
                "terminal.ansi.bright_green": "#F730F5",
                "terminal.ansi.bright_yellow": "#F461ED",
                "terminal.ansi.bright_blue": "#69A6F3",
                "terminal.ansi.bright_magenta": "#FC9F99",
                "terminal.ansi.bright_cyan": "#B4B8E0",
                "terminal.ansi.bright_white": "#c4c3c6",
                "modified": "#F05B9A",
                "syntax": {
                    "attribute": {
                        "color": "#F05B9A"
                    },
                    "boolean": {
                      "color": "#F730F5"
                    },
                    "comment": {
                      "color": "#605d71"
                    },
                    "comment.doc": {
                      "color": "#605d71"
                    },
                    "constant": {
                      "color": "#F730F5"
                    },
                    "function": {
                      "color": "#B4B8E0"
                    },
                    "keyword": {
                      "color": "#69A6F3"
                    },
                    "number": {
                      "color": "#FC9F99"
                    },
                    "operator": {
                      "color": "#69A6F3"
                    },
                    "string": {
                      "color": "#F05B9A"
                    },
                    "variable": {
                      "color": "#F730F5"
                    }
                },
                "players": [
                  {
                    "cursor": "#F05B9A",
                    "background": "#000000",
                    "selection": "#F05B9A30"
                  }
                ]
            }
        }
    ]
}
