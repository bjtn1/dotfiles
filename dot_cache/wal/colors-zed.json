{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c5c5ca",
                "text": "#c5c5ca",
                "text.muted": "#c5c5ca70",
                "text.ignored": "#c5c5ca40",
                "text.placeholder": "#c5c5ca50",
                "ignored": "#c5c5ca30",
                "element.hover": "#c5c5ca30",
                "ghost_element.hover": "#5D92C330",
                "ghost_element.selected": "#5D92C330",
                "ghost_element.active": "#5D92C360",
                "border": "#000000",
                "editor.highlighted_line.background": "#5D92C310",
                "editor.active_line.background": "#5D92C310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#5D92C330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#5D92C3",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c5c5ca",
                "terminal.dim_foreground": "#c5c5ca",
                "terminal.bright_foreground": "#c5c5ca",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#5D92C3",
                "terminal.ansi.green": "#AE86A7",
                "terminal.ansi.yellow": "#E0929D",
                "terminal.ansi.blue": "#FDD5AC",
                "terminal.ansi.magenta": "#A4B9D5",
                "terminal.ansi.cyan": "#DAD8DC",
                "terminal.ansi.white": "#c5c5ca",
                "terminal.ansi.bright_black": "#656379",
                "terminal.ansi.bright_red": "#5D92C3",
                "terminal.ansi.bright_green": "#AE86A7",
                "terminal.ansi.bright_yellow": "#E0929D",
                "terminal.ansi.bright_blue": "#FDD5AC",
                "terminal.ansi.bright_magenta": "#A4B9D5",
                "terminal.ansi.bright_cyan": "#DAD8DC",
                "terminal.ansi.bright_white": "#c5c5ca",
                "modified": "#5D92C3",
                "syntax": {
                    "attribute": {
                        "color": "#5D92C3"
                    },
                    "boolean": {
                      "color": "#AE86A7"
                    },
                    "comment": {
                      "color": "#656379"
                    },
                    "comment.doc": {
                      "color": "#656379"
                    },
                    "constant": {
                      "color": "#AE86A7"
                    },
                    "function": {
                      "color": "#DAD8DC"
                    },
                    "keyword": {
                      "color": "#FDD5AC"
                    },
                    "number": {
                      "color": "#A4B9D5"
                    },
                    "operator": {
                      "color": "#FDD5AC"
                    },
                    "string": {
                      "color": "#5D92C3"
                    },
                    "variable": {
                      "color": "#AE86A7"
                    }
                },
                "players": [
                  {
                    "cursor": "#5D92C3",
                    "background": "#000000",
                    "selection": "#5D92C330"
                  }
                ]
            }
        }
    ]
}
