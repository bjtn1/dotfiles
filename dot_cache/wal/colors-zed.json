{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c6c6",
                "text": "#c3c6c6",
                "text.muted": "#c3c6c670",
                "text.ignored": "#c3c6c640",
                "text.placeholder": "#c3c6c650",
                "ignored": "#c3c6c630",
                "element.hover": "#c3c6c630",
                "ghost_element.hover": "#AB674E30",
                "ghost_element.selected": "#AB674E30",
                "ghost_element.active": "#AB674E60",
                "border": "#000000",
                "editor.highlighted_line.background": "#AB674E10",
                "editor.active_line.background": "#AB674E10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#AB674E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#AB674E",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c6c6",
                "terminal.dim_foreground": "#c3c6c6",
                "terminal.bright_foreground": "#c3c6c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#AB674E",
                "terminal.ansi.green": "#548654",
                "terminal.ansi.yellow": "#6B8D66",
                "terminal.ansi.blue": "#A0996D",
                "terminal.ansi.magenta": "#547588",
                "terminal.ansi.cyan": "#609BA5",
                "terminal.ansi.white": "#c3c6c6",
                "terminal.ansi.bright_black": "#5d716d",
                "terminal.ansi.bright_red": "#AB674E",
                "terminal.ansi.bright_green": "#548654",
                "terminal.ansi.bright_yellow": "#6B8D66",
                "terminal.ansi.bright_blue": "#A0996D",
                "terminal.ansi.bright_magenta": "#547588",
                "terminal.ansi.bright_cyan": "#609BA5",
                "terminal.ansi.bright_white": "#c3c6c6",
                "modified": "#AB674E",
                "syntax": {
                    "attribute": {
                        "color": "#AB674E"
                    },
                    "boolean": {
                      "color": "#548654"
                    },
                    "comment": {
                      "color": "#5d716d"
                    },
                    "comment.doc": {
                      "color": "#5d716d"
                    },
                    "constant": {
                      "color": "#548654"
                    },
                    "function": {
                      "color": "#609BA5"
                    },
                    "keyword": {
                      "color": "#A0996D"
                    },
                    "number": {
                      "color": "#547588"
                    },
                    "operator": {
                      "color": "#A0996D"
                    },
                    "string": {
                      "color": "#AB674E"
                    },
                    "variable": {
                      "color": "#548654"
                    }
                },
                "players": [
                  {
                    "cursor": "#AB674E",
                    "background": "#000000",
                    "selection": "#AB674E30"
                  }
                ]
            }
        }
    ]
}
