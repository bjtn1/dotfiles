{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c2c6",
                "text": "#c3c2c6",
                "text.muted": "#c3c2c670",
                "text.ignored": "#c3c2c640",
                "text.placeholder": "#c3c2c650",
                "ignored": "#c3c2c630",
                "element.hover": "#c3c2c630",
                "ghost_element.hover": "#B7867B30",
                "ghost_element.selected": "#B7867B30",
                "ghost_element.active": "#B7867B60",
                "border": "#000000",
                "editor.highlighted_line.background": "#B7867B10",
                "editor.active_line.background": "#B7867B10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#B7867B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#B7867B",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c2c6",
                "terminal.dim_foreground": "#c3c2c6",
                "terminal.bright_foreground": "#c3c2c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#B7867B",
                "terminal.ansi.green": "#736C8B",
                "terminal.ansi.yellow": "#8F7596",
                "terminal.ansi.blue": "#A07692",
                "terminal.ansi.magenta": "#CA7984",
                "terminal.ansi.cyan": "#A3869A",
                "terminal.ansi.white": "#c3c2c6",
                "terminal.ansi.bright_black": "#5f5c70",
                "terminal.ansi.bright_red": "#B7867B",
                "terminal.ansi.bright_green": "#736C8B",
                "terminal.ansi.bright_yellow": "#8F7596",
                "terminal.ansi.bright_blue": "#A07692",
                "terminal.ansi.bright_magenta": "#CA7984",
                "terminal.ansi.bright_cyan": "#A3869A",
                "terminal.ansi.bright_white": "#c3c2c6",
                "modified": "#B7867B",
                "syntax": {
                    "attribute": {
                        "color": "#B7867B"
                    },
                    "boolean": {
                      "color": "#736C8B"
                    },
                    "comment": {
                      "color": "#5f5c70"
                    },
                    "comment.doc": {
                      "color": "#5f5c70"
                    },
                    "constant": {
                      "color": "#736C8B"
                    },
                    "function": {
                      "color": "#A3869A"
                    },
                    "keyword": {
                      "color": "#A07692"
                    },
                    "number": {
                      "color": "#CA7984"
                    },
                    "operator": {
                      "color": "#A07692"
                    },
                    "string": {
                      "color": "#B7867B"
                    },
                    "variable": {
                      "color": "#736C8B"
                    }
                },
                "players": [
                  {
                    "cursor": "#B7867B",
                    "background": "#000000",
                    "selection": "#B7867B30"
                  }
                ]
            }
        }
    ]
}
