{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c8",
                "text": "#c3c4c8",
                "text.muted": "#c3c4c870",
                "text.ignored": "#c3c4c840",
                "text.placeholder": "#c3c4c850",
                "ignored": "#c3c4c830",
                "element.hover": "#c3c4c830",
                "ghost_element.hover": "#29699330",
                "ghost_element.selected": "#29699330",
                "ghost_element.active": "#29699360",
                "border": "#000000",
                "editor.highlighted_line.background": "#29699310",
                "editor.active_line.background": "#29699310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#29699330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#296993",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c8",
                "terminal.dim_foreground": "#c3c4c8",
                "terminal.bright_foreground": "#c3c4c8",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#296993",
                "terminal.ansi.green": "#2273BF",
                "terminal.ansi.yellow": "#596EA2",
                "terminal.ansi.blue": "#A277A1",
                "terminal.ansi.magenta": "#358AB4",
                "terminal.ansi.cyan": "#37A2CB",
                "terminal.ansi.white": "#c3c4c8",
                "terminal.ansi.bright_black": "#5f6374",
                "terminal.ansi.bright_red": "#296993",
                "terminal.ansi.bright_green": "#2273BF",
                "terminal.ansi.bright_yellow": "#596EA2",
                "terminal.ansi.bright_blue": "#A277A1",
                "terminal.ansi.bright_magenta": "#358AB4",
                "terminal.ansi.bright_cyan": "#37A2CB",
                "terminal.ansi.bright_white": "#c3c4c8",
                "modified": "#296993",
                "syntax": {
                    "attribute": {
                        "color": "#296993"
                    },
                    "boolean": {
                      "color": "#2273BF"
                    },
                    "comment": {
                      "color": "#5f6374"
                    },
                    "comment.doc": {
                      "color": "#5f6374"
                    },
                    "constant": {
                      "color": "#2273BF"
                    },
                    "function": {
                      "color": "#37A2CB"
                    },
                    "keyword": {
                      "color": "#A277A1"
                    },
                    "number": {
                      "color": "#358AB4"
                    },
                    "operator": {
                      "color": "#A277A1"
                    },
                    "string": {
                      "color": "#296993"
                    },
                    "variable": {
                      "color": "#2273BF"
                    }
                },
                "players": [
                  {
                    "cursor": "#296993",
                    "background": "#000000",
                    "selection": "#29699330"
                  }
                ]
            }
        }
    ]
}
