{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c3c6",
                "text": "#c2c3c6",
                "text.muted": "#c2c3c670",
                "text.ignored": "#c2c3c640",
                "text.placeholder": "#c2c3c650",
                "ignored": "#c2c3c630",
                "element.hover": "#c2c3c630",
                "ghost_element.hover": "#5B525C30",
                "ghost_element.selected": "#5B525C30",
                "ghost_element.active": "#5B525C60",
                "border": "#000000",
                "editor.highlighted_line.background": "#5B525C10",
                "editor.active_line.background": "#5B525C10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#5B525C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#5B525C",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c3c6",
                "terminal.dim_foreground": "#c2c3c6",
                "terminal.bright_foreground": "#c2c3c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#5B525C",
                "terminal.ansi.green": "#9F6553",
                "terminal.ansi.yellow": "#D19D6B",
                "terminal.ansi.blue": "#2F53A5",
                "terminal.ansi.magenta": "#9B5FAC",
                "terminal.ansi.cyan": "#52A4E4",
                "terminal.ansi.white": "#c2c3c6",
                "terminal.ansi.bright_black": "#5c5f70",
                "terminal.ansi.bright_red": "#5B525C",
                "terminal.ansi.bright_green": "#9F6553",
                "terminal.ansi.bright_yellow": "#D19D6B",
                "terminal.ansi.bright_blue": "#2F53A5",
                "terminal.ansi.bright_magenta": "#9B5FAC",
                "terminal.ansi.bright_cyan": "#52A4E4",
                "terminal.ansi.bright_white": "#c2c3c6",
                "modified": "#5B525C",
                "syntax": {
                    "attribute": {
                        "color": "#5B525C"
                    },
                    "boolean": {
                      "color": "#9F6553"
                    },
                    "comment": {
                      "color": "#5c5f70"
                    },
                    "comment.doc": {
                      "color": "#5c5f70"
                    },
                    "constant": {
                      "color": "#9F6553"
                    },
                    "function": {
                      "color": "#52A4E4"
                    },
                    "keyword": {
                      "color": "#2F53A5"
                    },
                    "number": {
                      "color": "#9B5FAC"
                    },
                    "operator": {
                      "color": "#2F53A5"
                    },
                    "string": {
                      "color": "#5B525C"
                    },
                    "variable": {
                      "color": "#9F6553"
                    }
                },
                "players": [
                  {
                    "cursor": "#5B525C",
                    "background": "#000000",
                    "selection": "#5B525C30"
                  }
                ]
            }
        }
    ]
}
