{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c8cccc",
                "text": "#c8cccc",
                "text.muted": "#c8cccc70",
                "text.ignored": "#c8cccc40",
                "text.placeholder": "#c8cccc50",
                "ignored": "#c8cccc30",
                "element.hover": "#c8cccc30",
                "ghost_element.hover": "#1399AF30",
                "ghost_element.selected": "#1399AF30",
                "ghost_element.active": "#1399AF60",
                "border": "#000000",
                "editor.highlighted_line.background": "#1399AF10",
                "editor.active_line.background": "#1399AF10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#1399AF30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#1399AF",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c8cccc",
                "terminal.dim_foreground": "#c8cccc",
                "terminal.bright_foreground": "#c8cccc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#1399AF",
                "terminal.ansi.green": "#2C9FA9",
                "terminal.ansi.yellow": "#62AFA6",
                "terminal.ansi.blue": "#7EC1AE",
                "terminal.ansi.magenta": "#8BBCAA",
                "terminal.ansi.cyan": "#C8AC8A",
                "terminal.ansi.white": "#c8cccc",
                "terminal.ansi.bright_black": "#6a827e",
                "terminal.ansi.bright_red": "#1399AF",
                "terminal.ansi.bright_green": "#2C9FA9",
                "terminal.ansi.bright_yellow": "#62AFA6",
                "terminal.ansi.bright_blue": "#7EC1AE",
                "terminal.ansi.bright_magenta": "#8BBCAA",
                "terminal.ansi.bright_cyan": "#C8AC8A",
                "terminal.ansi.bright_white": "#c8cccc",
                "modified": "#1399AF",
                "syntax": {
                    "attribute": {
                        "color": "#1399AF"
                    },
                    "boolean": {
                      "color": "#2C9FA9"
                    },
                    "comment": {
                      "color": "#6a827e"
                    },
                    "comment.doc": {
                      "color": "#6a827e"
                    },
                    "constant": {
                      "color": "#2C9FA9"
                    },
                    "function": {
                      "color": "#C8AC8A"
                    },
                    "keyword": {
                      "color": "#7EC1AE"
                    },
                    "number": {
                      "color": "#8BBCAA"
                    },
                    "operator": {
                      "color": "#7EC1AE"
                    },
                    "string": {
                      "color": "#1399AF"
                    },
                    "variable": {
                      "color": "#2C9FA9"
                    }
                },
                "players": [
                  {
                    "cursor": "#1399AF",
                    "background": "#000000",
                    "selection": "#1399AF30"
                  }
                ]
            }
        }
    ]
}
