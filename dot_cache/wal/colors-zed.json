{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6c7c9",
                "text": "#c6c7c9",
                "text.muted": "#c6c7c970",
                "text.ignored": "#c6c7c940",
                "text.placeholder": "#c6c7c950",
                "ignored": "#c6c7c930",
                "element.hover": "#c6c7c930",
                "ghost_element.hover": "#C6C4B330",
                "ghost_element.selected": "#C6C4B330",
                "ghost_element.active": "#C6C4B360",
                "border": "#000000",
                "editor.highlighted_line.background": "#C6C4B310",
                "editor.active_line.background": "#C6C4B310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#C6C4B330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#C6C4B3",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6c7c9",
                "terminal.dim_foreground": "#c6c7c9",
                "terminal.bright_foreground": "#c6c7c9",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#C6C4B3",
                "terminal.ansi.green": "#A4B9C5",
                "terminal.ansi.yellow": "#B5CBCF",
                "terminal.ansi.blue": "#BEE6E8",
                "terminal.ansi.magenta": "#C9D6D4",
                "terminal.ansi.cyan": "#CFECED",
                "terminal.ansi.white": "#c6c7c9",
                "terminal.ansi.bright_black": "#636e7a",
                "terminal.ansi.bright_red": "#C6C4B3",
                "terminal.ansi.bright_green": "#A4B9C5",
                "terminal.ansi.bright_yellow": "#B5CBCF",
                "terminal.ansi.bright_blue": "#BEE6E8",
                "terminal.ansi.bright_magenta": "#C9D6D4",
                "terminal.ansi.bright_cyan": "#CFECED",
                "terminal.ansi.bright_white": "#c6c7c9",
                "modified": "#C6C4B3",
                "syntax": {
                    "attribute": {
                        "color": "#C6C4B3"
                    },
                    "boolean": {
                      "color": "#A4B9C5"
                    },
                    "comment": {
                      "color": "#636e7a"
                    },
                    "comment.doc": {
                      "color": "#636e7a"
                    },
                    "constant": {
                      "color": "#A4B9C5"
                    },
                    "function": {
                      "color": "#CFECED"
                    },
                    "keyword": {
                      "color": "#BEE6E8"
                    },
                    "number": {
                      "color": "#C9D6D4"
                    },
                    "operator": {
                      "color": "#BEE6E8"
                    },
                    "string": {
                      "color": "#C6C4B3"
                    },
                    "variable": {
                      "color": "#A4B9C5"
                    }
                },
                "players": [
                  {
                    "cursor": "#C6C4B3",
                    "background": "#000000",
                    "selection": "#C6C4B330"
                  }
                ]
            }
        }
    ]
}
