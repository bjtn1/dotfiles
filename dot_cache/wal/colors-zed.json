{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c1c1c5",
                "text": "#c1c1c5",
                "text.muted": "#c1c1c570",
                "text.ignored": "#c1c1c540",
                "text.placeholder": "#c1c1c550",
                "ignored": "#c1c1c530",
                "element.hover": "#c1c1c530",
                "ghost_element.hover": "#50506530",
                "ghost_element.selected": "#50506530",
                "ghost_element.active": "#50506560",
                "border": "#000000",
                "editor.highlighted_line.background": "#50506510",
                "editor.active_line.background": "#50506510",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#50506530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#505065",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c1c1c5",
                "terminal.dim_foreground": "#c1c1c5",
                "terminal.bright_foreground": "#c1c1c5",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#505065",
                "terminal.ansi.green": "#535267",
                "terminal.ansi.yellow": "#6E6678",
                "terminal.ansi.blue": "#82727C",
                "terminal.ansi.magenta": "#7D7181",
                "terminal.ansi.cyan": "#887988",
                "terminal.ansi.white": "#c1c1c5",
                "terminal.ansi.bright_black": "#59596d",
                "terminal.ansi.bright_red": "#505065",
                "terminal.ansi.bright_green": "#535267",
                "terminal.ansi.bright_yellow": "#6E6678",
                "terminal.ansi.bright_blue": "#82727C",
                "terminal.ansi.bright_magenta": "#7D7181",
                "terminal.ansi.bright_cyan": "#887988",
                "terminal.ansi.bright_white": "#c1c1c5",
                "modified": "#505065",
                "syntax": {
                    "attribute": {
                        "color": "#505065"
                    },
                    "boolean": {
                      "color": "#535267"
                    },
                    "comment": {
                      "color": "#59596d"
                    },
                    "comment.doc": {
                      "color": "#59596d"
                    },
                    "constant": {
                      "color": "#535267"
                    },
                    "function": {
                      "color": "#887988"
                    },
                    "keyword": {
                      "color": "#82727C"
                    },
                    "number": {
                      "color": "#7D7181"
                    },
                    "operator": {
                      "color": "#82727C"
                    },
                    "string": {
                      "color": "#505065"
                    },
                    "variable": {
                      "color": "#535267"
                    }
                },
                "players": [
                  {
                    "cursor": "#505065",
                    "background": "#000000",
                    "selection": "#50506530"
                  }
                ]
            }
        }
    ]
}
