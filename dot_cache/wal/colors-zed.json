{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c1c4c5",
                "text": "#c1c4c5",
                "text.muted": "#c1c4c570",
                "text.ignored": "#c1c4c540",
                "text.placeholder": "#c1c4c550",
                "ignored": "#c1c4c530",
                "element.hover": "#c1c4c530",
                "ghost_element.hover": "#02695830",
                "ghost_element.selected": "#02695830",
                "ghost_element.active": "#02695860",
                "border": "#000000",
                "editor.highlighted_line.background": "#02695810",
                "editor.active_line.background": "#02695810",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#02695830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#026958",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c1c4c5",
                "terminal.dim_foreground": "#c1c4c5",
                "terminal.bright_foreground": "#c1c4c5",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#026958",
                "terminal.ansi.green": "#01536E",
                "terminal.ansi.yellow": "#01647B",
                "terminal.ansi.blue": "#028F76",
                "terminal.ansi.magenta": "#016988",
                "terminal.ansi.cyan": "#026799",
                "terminal.ansi.white": "#c1c4c5",
                "terminal.ansi.bright_black": "#596b6d",
                "terminal.ansi.bright_red": "#026958",
                "terminal.ansi.bright_green": "#01536E",
                "terminal.ansi.bright_yellow": "#01647B",
                "terminal.ansi.bright_blue": "#028F76",
                "terminal.ansi.bright_magenta": "#016988",
                "terminal.ansi.bright_cyan": "#026799",
                "terminal.ansi.bright_white": "#c1c4c5",
                "modified": "#026958",
                "syntax": {
                    "attribute": {
                        "color": "#026958"
                    },
                    "boolean": {
                      "color": "#01536E"
                    },
                    "comment": {
                      "color": "#596b6d"
                    },
                    "comment.doc": {
                      "color": "#596b6d"
                    },
                    "constant": {
                      "color": "#01536E"
                    },
                    "function": {
                      "color": "#026799"
                    },
                    "keyword": {
                      "color": "#028F76"
                    },
                    "number": {
                      "color": "#016988"
                    },
                    "operator": {
                      "color": "#028F76"
                    },
                    "string": {
                      "color": "#026958"
                    },
                    "variable": {
                      "color": "#01536E"
                    }
                },
                "players": [
                  {
                    "cursor": "#026958",
                    "background": "#000000",
                    "selection": "#02695830"
                  }
                ]
            }
        }
    ]
}
