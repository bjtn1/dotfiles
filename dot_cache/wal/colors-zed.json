{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6cacf",
                "text": "#c6cacf",
                "text.muted": "#c6cacf70",
                "text.ignored": "#c6cacf40",
                "text.placeholder": "#c6cacf50",
                "ignored": "#c6cacf30",
                "element.hover": "#c6cacf30",
                "ghost_element.hover": "#3272A930",
                "ghost_element.selected": "#3272A930",
                "ghost_element.active": "#3272A960",
                "border": "#000000",
                "editor.highlighted_line.background": "#3272A910",
                "editor.active_line.background": "#3272A910",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#3272A930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#3272A9",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6cacf",
                "terminal.dim_foreground": "#c6cacf",
                "terminal.bright_foreground": "#c6cacf",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#3272A9",
                "terminal.ansi.green": "#3894BD",
                "terminal.ansi.yellow": "#15A2D9",
                "terminal.ansi.blue": "#28CBF4",
                "terminal.ansi.magenta": "#5DD3F4",
                "terminal.ansi.cyan": "#9EA7B9",
                "terminal.ansi.white": "#c6cacf",
                "terminal.ansi.bright_black": "#6a7581",
                "terminal.ansi.bright_red": "#3272A9",
                "terminal.ansi.bright_green": "#3894BD",
                "terminal.ansi.bright_yellow": "#15A2D9",
                "terminal.ansi.bright_blue": "#28CBF4",
                "terminal.ansi.bright_magenta": "#5DD3F4",
                "terminal.ansi.bright_cyan": "#9EA7B9",
                "terminal.ansi.bright_white": "#c6cacf",
                "modified": "#3272A9",
                "syntax": {
                    "attribute": {
                        "color": "#3272A9"
                    },
                    "boolean": {
                      "color": "#3894BD"
                    },
                    "comment": {
                      "color": "#6a7581"
                    },
                    "comment.doc": {
                      "color": "#6a7581"
                    },
                    "constant": {
                      "color": "#3894BD"
                    },
                    "function": {
                      "color": "#9EA7B9"
                    },
                    "keyword": {
                      "color": "#28CBF4"
                    },
                    "number": {
                      "color": "#5DD3F4"
                    },
                    "operator": {
                      "color": "#28CBF4"
                    },
                    "string": {
                      "color": "#3272A9"
                    },
                    "variable": {
                      "color": "#3894BD"
                    }
                },
                "players": [
                  {
                    "cursor": "#3272A9",
                    "background": "#000000",
                    "selection": "#3272A930"
                  }
                ]
            }
        }
    ]
}
