{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6caca",
                "text": "#c6caca",
                "text.muted": "#c6caca70",
                "text.ignored": "#c6caca40",
                "text.placeholder": "#c6caca50",
                "ignored": "#c6caca30",
                "element.hover": "#c6caca30",
                "ghost_element.hover": "#4B95AE30",
                "ghost_element.selected": "#4B95AE30",
                "ghost_element.active": "#4B95AE60",
                "border": "#000000",
                "editor.highlighted_line.background": "#4B95AE10",
                "editor.active_line.background": "#4B95AE10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#4B95AE30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#4B95AE",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6caca",
                "terminal.dim_foreground": "#c6caca",
                "terminal.bright_foreground": "#c6caca",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#4B95AE",
                "terminal.ansi.green": "#57A4A8",
                "terminal.ansi.yellow": "#9CACA2",
                "terminal.ansi.blue": "#E5ACA1",
                "terminal.ansi.magenta": "#A8C8AA",
                "terminal.ansi.cyan": "#DDCDB2",
                "terminal.ansi.white": "#c6caca",
                "terminal.ansi.bright_black": "#647a7a",
                "terminal.ansi.bright_red": "#4B95AE",
                "terminal.ansi.bright_green": "#57A4A8",
                "terminal.ansi.bright_yellow": "#9CACA2",
                "terminal.ansi.bright_blue": "#E5ACA1",
                "terminal.ansi.bright_magenta": "#A8C8AA",
                "terminal.ansi.bright_cyan": "#DDCDB2",
                "terminal.ansi.bright_white": "#c6caca",
                "modified": "#4B95AE",
                "syntax": {
                    "attribute": {
                        "color": "#4B95AE"
                    },
                    "boolean": {
                      "color": "#57A4A8"
                    },
                    "comment": {
                      "color": "#647a7a"
                    },
                    "comment.doc": {
                      "color": "#647a7a"
                    },
                    "constant": {
                      "color": "#57A4A8"
                    },
                    "function": {
                      "color": "#DDCDB2"
                    },
                    "keyword": {
                      "color": "#E5ACA1"
                    },
                    "number": {
                      "color": "#A8C8AA"
                    },
                    "operator": {
                      "color": "#E5ACA1"
                    },
                    "string": {
                      "color": "#4B95AE"
                    },
                    "variable": {
                      "color": "#57A4A8"
                    }
                },
                "players": [
                  {
                    "cursor": "#4B95AE",
                    "background": "#000000",
                    "selection": "#4B95AE30"
                  }
                ]
            }
        }
    ]
}
