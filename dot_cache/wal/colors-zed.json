{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c4c7",
                "text": "#c2c4c7",
                "text.muted": "#c2c4c770",
                "text.ignored": "#c2c4c740",
                "text.placeholder": "#c2c4c750",
                "ignored": "#c2c4c730",
                "element.hover": "#c2c4c730",
                "ghost_element.hover": "#33485330",
                "ghost_element.selected": "#33485330",
                "ghost_element.active": "#33485360",
                "border": "#000000",
                "editor.highlighted_line.background": "#33485310",
                "editor.active_line.background": "#33485310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#33485330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#334853",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c4c7",
                "terminal.dim_foreground": "#c2c4c7",
                "terminal.bright_foreground": "#c2c4c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#334853",
                "terminal.ansi.green": "#3B5364",
                "terminal.ansi.yellow": "#565E5F",
                "terminal.ansi.blue": "#8E725B",
                "terminal.ansi.magenta": "#A1906C",
                "terminal.ansi.cyan": "#6A7C81",
                "terminal.ansi.white": "#c2c4c7",
                "terminal.ansi.bright_black": "#5c6571",
                "terminal.ansi.bright_red": "#334853",
                "terminal.ansi.bright_green": "#3B5364",
                "terminal.ansi.bright_yellow": "#565E5F",
                "terminal.ansi.bright_blue": "#8E725B",
                "terminal.ansi.bright_magenta": "#A1906C",
                "terminal.ansi.bright_cyan": "#6A7C81",
                "terminal.ansi.bright_white": "#c2c4c7",
                "modified": "#334853",
                "syntax": {
                    "attribute": {
                        "color": "#334853"
                    },
                    "boolean": {
                      "color": "#3B5364"
                    },
                    "comment": {
                      "color": "#5c6571"
                    },
                    "comment.doc": {
                      "color": "#5c6571"
                    },
                    "constant": {
                      "color": "#3B5364"
                    },
                    "function": {
                      "color": "#6A7C81"
                    },
                    "keyword": {
                      "color": "#8E725B"
                    },
                    "number": {
                      "color": "#A1906C"
                    },
                    "operator": {
                      "color": "#8E725B"
                    },
                    "string": {
                      "color": "#334853"
                    },
                    "variable": {
                      "color": "#3B5364"
                    }
                },
                "players": [
                  {
                    "cursor": "#334853",
                    "background": "#000000",
                    "selection": "#33485330"
                  }
                ]
            }
        }
    ]
}
