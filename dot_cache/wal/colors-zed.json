{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#ddd1d8",
                "text": "#ddd1d8",
                "text.muted": "#ddd1d870",
                "text.ignored": "#ddd1d840",
                "text.placeholder": "#ddd1d850",
                "ignored": "#ddd1d830",
                "element.hover": "#ddd1d830",
                "ghost_element.hover": "#B19CC530",
                "ghost_element.selected": "#B19CC530",
                "ghost_element.active": "#B19CC560",
                "border": "#000000",
                "editor.highlighted_line.background": "#B19CC510",
                "editor.active_line.background": "#B19CC510",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#B19CC530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#B19CC5",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#ddd1d8",
                "terminal.dim_foreground": "#ddd1d8",
                "terminal.bright_foreground": "#ddd1d8",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#B19CC5",
                "terminal.ansi.green": "#92ABD8",
                "terminal.ansi.yellow": "#AEB0D4",
                "terminal.ansi.blue": "#CB9DC2",
                "terminal.ansi.magenta": "#D0ADC8",
                "terminal.ansi.cyan": "#EFB3C5",
                "terminal.ansi.white": "#ddd1d8",
                "terminal.ansi.bright_black": "#a18d99",
                "terminal.ansi.bright_red": "#B19CC5",
                "terminal.ansi.bright_green": "#92ABD8",
                "terminal.ansi.bright_yellow": "#AEB0D4",
                "terminal.ansi.bright_blue": "#CB9DC2",
                "terminal.ansi.bright_magenta": "#D0ADC8",
                "terminal.ansi.bright_cyan": "#EFB3C5",
                "terminal.ansi.bright_white": "#ddd1d8",
                "modified": "#B19CC5",
                "syntax": {
                    "attribute": {
                        "color": "#B19CC5"
                    },
                    "boolean": {
                      "color": "#92ABD8"
                    },
                    "comment": {
                      "color": "#a18d99"
                    },
                    "comment.doc": {
                      "color": "#a18d99"
                    },
                    "constant": {
                      "color": "#92ABD8"
                    },
                    "function": {
                      "color": "#EFB3C5"
                    },
                    "keyword": {
                      "color": "#CB9DC2"
                    },
                    "number": {
                      "color": "#D0ADC8"
                    },
                    "operator": {
                      "color": "#CB9DC2"
                    },
                    "string": {
                      "color": "#B19CC5"
                    },
                    "variable": {
                      "color": "#92ABD8"
                    }
                },
                "players": [
                  {
                    "cursor": "#B19CC5",
                    "background": "#000000",
                    "selection": "#B19CC530"
                  }
                ]
            }
        }
    ]
}
