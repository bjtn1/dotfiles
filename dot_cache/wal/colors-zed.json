{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4cacc",
                "text": "#c4cacc",
                "text.muted": "#c4cacc70",
                "text.ignored": "#c4cacc40",
                "text.placeholder": "#c4cacc50",
                "ignored": "#c4cacc30",
                "element.hover": "#c4cacc30",
                "ghost_element.hover": "#134C8930",
                "ghost_element.selected": "#134C8930",
                "ghost_element.active": "#134C8960",
                "border": "#000000",
                "editor.highlighted_line.background": "#134C8910",
                "editor.active_line.background": "#134C8910",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#134C8930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#134C89",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4cacc",
                "terminal.dim_foreground": "#c4cacc",
                "terminal.bright_foreground": "#c4cacc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#134C89",
                "terminal.ansi.green": "#265B93",
                "terminal.ansi.yellow": "#2B50A0",
                "terminal.ansi.blue": "#5B629A",
                "terminal.ansi.magenta": "#9C7099",
                "terminal.ansi.cyan": "#D47A9A",
                "terminal.ansi.white": "#c4cacc",
                "terminal.ansi.bright_black": "#65777c",
                "terminal.ansi.bright_red": "#134C89",
                "terminal.ansi.bright_green": "#265B93",
                "terminal.ansi.bright_yellow": "#2B50A0",
                "terminal.ansi.bright_blue": "#5B629A",
                "terminal.ansi.bright_magenta": "#9C7099",
                "terminal.ansi.bright_cyan": "#D47A9A",
                "terminal.ansi.bright_white": "#c4cacc",
                "modified": "#134C89",
                "syntax": {
                    "attribute": {
                        "color": "#134C89"
                    },
                    "boolean": {
                      "color": "#265B93"
                    },
                    "comment": {
                      "color": "#65777c"
                    },
                    "comment.doc": {
                      "color": "#65777c"
                    },
                    "constant": {
                      "color": "#265B93"
                    },
                    "function": {
                      "color": "#D47A9A"
                    },
                    "keyword": {
                      "color": "#5B629A"
                    },
                    "number": {
                      "color": "#9C7099"
                    },
                    "operator": {
                      "color": "#5B629A"
                    },
                    "string": {
                      "color": "#134C89"
                    },
                    "variable": {
                      "color": "#265B93"
                    }
                },
                "players": [
                  {
                    "cursor": "#134C89",
                    "background": "#000000",
                    "selection": "#134C8930"
                  }
                ]
            }
        }
    ]
}
