{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c7c7",
                "text": "#c2c7c7",
                "text.muted": "#c2c7c770",
                "text.ignored": "#c2c7c740",
                "text.placeholder": "#c2c7c750",
                "ignored": "#c2c7c730",
                "element.hover": "#c2c7c730",
                "ghost_element.hover": "#A5633630",
                "ghost_element.selected": "#A5633630",
                "ghost_element.active": "#A5633660",
                "border": "#000000",
                "editor.highlighted_line.background": "#A5633610",
                "editor.active_line.background": "#A5633610",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#A5633630",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#A56336",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c7c7",
                "terminal.dim_foreground": "#c2c7c7",
                "terminal.bright_foreground": "#c2c7c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#A56336",
                "terminal.ansi.green": "#5C906F",
                "terminal.ansi.yellow": "#C7AC58",
                "terminal.ansi.blue": "#3D738B",
                "terminal.ansi.magenta": "#6AA99A",
                "terminal.ansi.cyan": "#A3C3AC",
                "terminal.ansi.white": "#c2c7c7",
                "terminal.ansi.bright_black": "#5d7272",
                "terminal.ansi.bright_red": "#A56336",
                "terminal.ansi.bright_green": "#5C906F",
                "terminal.ansi.bright_yellow": "#C7AC58",
                "terminal.ansi.bright_blue": "#3D738B",
                "terminal.ansi.bright_magenta": "#6AA99A",
                "terminal.ansi.bright_cyan": "#A3C3AC",
                "terminal.ansi.bright_white": "#c2c7c7",
                "modified": "#A56336",
                "syntax": {
                    "attribute": {
                        "color": "#A56336"
                    },
                    "boolean": {
                      "color": "#5C906F"
                    },
                    "comment": {
                      "color": "#5d7272"
                    },
                    "comment.doc": {
                      "color": "#5d7272"
                    },
                    "constant": {
                      "color": "#5C906F"
                    },
                    "function": {
                      "color": "#A3C3AC"
                    },
                    "keyword": {
                      "color": "#3D738B"
                    },
                    "number": {
                      "color": "#6AA99A"
                    },
                    "operator": {
                      "color": "#3D738B"
                    },
                    "string": {
                      "color": "#A56336"
                    },
                    "variable": {
                      "color": "#5C906F"
                    }
                },
                "players": [
                  {
                    "cursor": "#A56336",
                    "background": "#000000",
                    "selection": "#A5633630"
                  }
                ]
            }
        }
    ]
}
