{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c3c7",
                "text": "#c2c3c7",
                "text.muted": "#c2c3c770",
                "text.ignored": "#c2c3c740",
                "text.placeholder": "#c2c3c750",
                "ignored": "#c2c3c730",
                "element.hover": "#c2c3c730",
                "ghost_element.hover": "#32669830",
                "ghost_element.selected": "#32669830",
                "ghost_element.active": "#32669860",
                "border": "#000000",
                "editor.highlighted_line.background": "#32669810",
                "editor.active_line.background": "#32669810",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#32669830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#326698",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c3c7",
                "terminal.dim_foreground": "#c2c3c7",
                "terminal.bright_foreground": "#c2c3c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#326698",
                "terminal.ansi.green": "#5D748C",
                "terminal.ansi.yellow": "#6E899A",
                "terminal.ansi.blue": "#4694A9",
                "terminal.ansi.magenta": "#96A0AA",
                "terminal.ansi.cyan": "#C8BEBA",
                "terminal.ansi.white": "#c2c3c7",
                "terminal.ansi.bright_black": "#5c6171",
                "terminal.ansi.bright_red": "#326698",
                "terminal.ansi.bright_green": "#5D748C",
                "terminal.ansi.bright_yellow": "#6E899A",
                "terminal.ansi.bright_blue": "#4694A9",
                "terminal.ansi.bright_magenta": "#96A0AA",
                "terminal.ansi.bright_cyan": "#C8BEBA",
                "terminal.ansi.bright_white": "#c2c3c7",
                "modified": "#326698",
                "syntax": {
                    "attribute": {
                        "color": "#326698"
                    },
                    "boolean": {
                      "color": "#5D748C"
                    },
                    "comment": {
                      "color": "#5c6171"
                    },
                    "comment.doc": {
                      "color": "#5c6171"
                    },
                    "constant": {
                      "color": "#5D748C"
                    },
                    "function": {
                      "color": "#C8BEBA"
                    },
                    "keyword": {
                      "color": "#4694A9"
                    },
                    "number": {
                      "color": "#96A0AA"
                    },
                    "operator": {
                      "color": "#4694A9"
                    },
                    "string": {
                      "color": "#326698"
                    },
                    "variable": {
                      "color": "#5D748C"
                    }
                },
                "players": [
                  {
                    "cursor": "#326698",
                    "background": "#000000",
                    "selection": "#32669830"
                  }
                ]
            }
        }
    ]
}
