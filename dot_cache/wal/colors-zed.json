{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c6cc",
                "text": "#c4c6cc",
                "text.muted": "#c4c6cc70",
                "text.ignored": "#c4c6cc40",
                "text.placeholder": "#c4c6cc50",
                "ignored": "#c4c6cc30",
                "element.hover": "#c4c6cc30",
                "ghost_element.hover": "#AC947230",
                "ghost_element.selected": "#AC947230",
                "ghost_element.active": "#AC947260",
                "border": "#000000",
                "editor.highlighted_line.background": "#AC947210",
                "editor.active_line.background": "#AC947210",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#AC947230",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#AC9472",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c6cc",
                "terminal.dim_foreground": "#c4c6cc",
                "terminal.bright_foreground": "#c4c6cc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#AC9472",
                "terminal.ansi.green": "#225BA1",
                "terminal.ansi.yellow": "#2857B2",
                "terminal.ansi.blue": "#5F66A0",
                "terminal.ansi.magenta": "#946D9D",
                "terminal.ansi.cyan": "#4B94C8",
                "terminal.ansi.white": "#c4c6cc",
                "terminal.ansi.bright_black": "#646b7b",
                "terminal.ansi.bright_red": "#AC9472",
                "terminal.ansi.bright_green": "#225BA1",
                "terminal.ansi.bright_yellow": "#2857B2",
                "terminal.ansi.bright_blue": "#5F66A0",
                "terminal.ansi.bright_magenta": "#946D9D",
                "terminal.ansi.bright_cyan": "#4B94C8",
                "terminal.ansi.bright_white": "#c4c6cc",
                "modified": "#AC9472",
                "syntax": {
                    "attribute": {
                        "color": "#AC9472"
                    },
                    "boolean": {
                      "color": "#225BA1"
                    },
                    "comment": {
                      "color": "#646b7b"
                    },
                    "comment.doc": {
                      "color": "#646b7b"
                    },
                    "constant": {
                      "color": "#225BA1"
                    },
                    "function": {
                      "color": "#4B94C8"
                    },
                    "keyword": {
                      "color": "#5F66A0"
                    },
                    "number": {
                      "color": "#946D9D"
                    },
                    "operator": {
                      "color": "#5F66A0"
                    },
                    "string": {
                      "color": "#AC9472"
                    },
                    "variable": {
                      "color": "#225BA1"
                    }
                },
                "players": [
                  {
                    "cursor": "#AC9472",
                    "background": "#000000",
                    "selection": "#AC947230"
                  }
                ]
            }
        }
    ]
}
