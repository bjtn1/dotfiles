{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c8c9d2",
                "text": "#c8c9d2",
                "text.muted": "#c8c9d270",
                "text.ignored": "#c8c9d240",
                "text.placeholder": "#c8c9d250",
                "ignored": "#c8c9d230",
                "element.hover": "#c8c9d230",
                "ghost_element.hover": "#3C82CE30",
                "ghost_element.selected": "#3C82CE30",
                "ghost_element.active": "#3C82CE60",
                "border": "#000000",
                "editor.highlighted_line.background": "#3C82CE10",
                "editor.active_line.background": "#3C82CE10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#3C82CE30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#3C82CE",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c8c9d2",
                "terminal.dim_foreground": "#c8c9d2",
                "terminal.bright_foreground": "#c8c9d2",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#3C82CE",
                "terminal.ansi.green": "#6693CE",
                "terminal.ansi.yellow": "#9D93B7",
                "terminal.ansi.blue": "#DFAFB7",
                "terminal.ansi.magenta": "#9EADD1",
                "terminal.ansi.cyan": "#D3B4CA",
                "terminal.ansi.white": "#c8c9d2",
                "terminal.ansi.bright_black": "#707389",
                "terminal.ansi.bright_red": "#3C82CE",
                "terminal.ansi.bright_green": "#6693CE",
                "terminal.ansi.bright_yellow": "#9D93B7",
                "terminal.ansi.bright_blue": "#DFAFB7",
                "terminal.ansi.bright_magenta": "#9EADD1",
                "terminal.ansi.bright_cyan": "#D3B4CA",
                "terminal.ansi.bright_white": "#c8c9d2",
                "modified": "#3C82CE",
                "syntax": {
                    "attribute": {
                        "color": "#3C82CE"
                    },
                    "boolean": {
                      "color": "#6693CE"
                    },
                    "comment": {
                      "color": "#707389"
                    },
                    "comment.doc": {
                      "color": "#707389"
                    },
                    "constant": {
                      "color": "#6693CE"
                    },
                    "function": {
                      "color": "#D3B4CA"
                    },
                    "keyword": {
                      "color": "#DFAFB7"
                    },
                    "number": {
                      "color": "#9EADD1"
                    },
                    "operator": {
                      "color": "#DFAFB7"
                    },
                    "string": {
                      "color": "#3C82CE"
                    },
                    "variable": {
                      "color": "#6693CE"
                    }
                },
                "players": [
                  {
                    "cursor": "#3C82CE",
                    "background": "#000000",
                    "selection": "#3C82CE30"
                  }
                ]
            }
        }
    ]
}
