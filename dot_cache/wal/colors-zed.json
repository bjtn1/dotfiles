{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c5c3c1",
                "text": "#c5c3c1",
                "text.muted": "#c5c3c170",
                "text.ignored": "#c5c3c140",
                "text.placeholder": "#c5c3c150",
                "ignored": "#c5c3c130",
                "element.hover": "#c5c3c130",
                "ghost_element.hover": "#6C523430",
                "ghost_element.selected": "#6C523430",
                "ghost_element.active": "#6C523460",
                "border": "#000000",
                "editor.highlighted_line.background": "#6C523410",
                "editor.active_line.background": "#6C523410",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#6C523430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#6C5234",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c5c3c1",
                "terminal.dim_foreground": "#c5c3c1",
                "terminal.bright_foreground": "#c5c3c1",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#6C5234",
                "terminal.ansi.green": "#3D4041",
                "terminal.ansi.yellow": "#504D4A",
                "terminal.ansi.blue": "#6A6056",
                "terminal.ansi.magenta": "#986833",
                "terminal.ansi.cyan": "#8C7454",
                "terminal.ansi.white": "#c5c3c1",
                "terminal.ansi.bright_black": "#6d6259",
                "terminal.ansi.bright_red": "#6C5234",
                "terminal.ansi.bright_green": "#3D4041",
                "terminal.ansi.bright_yellow": "#504D4A",
                "terminal.ansi.bright_blue": "#6A6056",
                "terminal.ansi.bright_magenta": "#986833",
                "terminal.ansi.bright_cyan": "#8C7454",
                "terminal.ansi.bright_white": "#c5c3c1",
                "modified": "#6C5234",
                "syntax": {
                    "attribute": {
                        "color": "#6C5234"
                    },
                    "boolean": {
                      "color": "#3D4041"
                    },
                    "comment": {
                      "color": "#6d6259"
                    },
                    "comment.doc": {
                      "color": "#6d6259"
                    },
                    "constant": {
                      "color": "#3D4041"
                    },
                    "function": {
                      "color": "#8C7454"
                    },
                    "keyword": {
                      "color": "#6A6056"
                    },
                    "number": {
                      "color": "#986833"
                    },
                    "operator": {
                      "color": "#6A6056"
                    },
                    "string": {
                      "color": "#6C5234"
                    },
                    "variable": {
                      "color": "#3D4041"
                    }
                },
                "players": [
                  {
                    "cursor": "#6C5234",
                    "background": "#000000",
                    "selection": "#6C523430"
                  }
                ]
            }
        }
    ]
}
