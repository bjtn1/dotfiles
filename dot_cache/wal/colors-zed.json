{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#cbcdd0",
                "text": "#cbcdd0",
                "text.muted": "#cbcdd070",
                "text.ignored": "#cbcdd040",
                "text.placeholder": "#cbcdd050",
                "ignored": "#cbcdd030",
                "element.hover": "#cbcdd030",
                "ghost_element.hover": "#C1AAAF30",
                "ghost_element.selected": "#C1AAAF30",
                "ghost_element.active": "#C1AAAF60",
                "border": "#000000",
                "editor.highlighted_line.background": "#C1AAAF10",
                "editor.active_line.background": "#C1AAAF10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#C1AAAF30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#C1AAAF",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#cbcdd0",
                "terminal.dim_foreground": "#cbcdd0",
                "terminal.bright_foreground": "#cbcdd0",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#C1AAAF",
                "terminal.ansi.green": "#96B4CE",
                "terminal.ansi.yellow": "#AAD3EA",
                "terminal.ansi.blue": "#F7DACC",
                "terminal.ansi.magenta": "#FEE7D4",
                "terminal.ansi.cyan": "#CED6E2",
                "terminal.ansi.white": "#cbcdd0",
                "terminal.ansi.bright_black": "#707e8a",
                "terminal.ansi.bright_red": "#C1AAAF",
                "terminal.ansi.bright_green": "#96B4CE",
                "terminal.ansi.bright_yellow": "#AAD3EA",
                "terminal.ansi.bright_blue": "#F7DACC",
                "terminal.ansi.bright_magenta": "#FEE7D4",
                "terminal.ansi.bright_cyan": "#CED6E2",
                "terminal.ansi.bright_white": "#cbcdd0",
                "modified": "#C1AAAF",
                "syntax": {
                    "attribute": {
                        "color": "#C1AAAF"
                    },
                    "boolean": {
                      "color": "#96B4CE"
                    },
                    "comment": {
                      "color": "#707e8a"
                    },
                    "comment.doc": {
                      "color": "#707e8a"
                    },
                    "constant": {
                      "color": "#96B4CE"
                    },
                    "function": {
                      "color": "#CED6E2"
                    },
                    "keyword": {
                      "color": "#F7DACC"
                    },
                    "number": {
                      "color": "#FEE7D4"
                    },
                    "operator": {
                      "color": "#F7DACC"
                    },
                    "string": {
                      "color": "#C1AAAF"
                    },
                    "variable": {
                      "color": "#96B4CE"
                    }
                },
                "players": [
                  {
                    "cursor": "#C1AAAF",
                    "background": "#000000",
                    "selection": "#C1AAAF30"
                  }
                ]
            }
        }
    ]
}
