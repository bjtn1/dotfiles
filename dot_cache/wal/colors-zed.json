{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c1c1",
                "text": "#c4c1c1",
                "text.muted": "#c4c1c170",
                "text.ignored": "#c4c1c140",
                "text.placeholder": "#c4c1c150",
                "ignored": "#c4c1c130",
                "element.hover": "#c4c1c130",
                "ghost_element.hover": "#C5473030",
                "ghost_element.selected": "#C5473030",
                "ghost_element.active": "#C5473060",
                "border": "#000000",
                "editor.highlighted_line.background": "#C5473010",
                "editor.active_line.background": "#C5473010",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#C5473030",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#C54730",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c1c1",
                "terminal.dim_foreground": "#c4c1c1",
                "terminal.bright_foreground": "#c4c1c1",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#C54730",
                "terminal.ansi.green": "#EC592C",
                "terminal.ansi.yellow": "#F09562",
                "terminal.ansi.blue": "#5F7C83",
                "terminal.ansi.magenta": "#6D8D91",
                "terminal.ansi.cyan": "#F6B08D",
                "terminal.ansi.white": "#c4c1c1",
                "terminal.ansi.bright_black": "#6c5858",
                "terminal.ansi.bright_red": "#C54730",
                "terminal.ansi.bright_green": "#EC592C",
                "terminal.ansi.bright_yellow": "#F09562",
                "terminal.ansi.bright_blue": "#5F7C83",
                "terminal.ansi.bright_magenta": "#6D8D91",
                "terminal.ansi.bright_cyan": "#F6B08D",
                "terminal.ansi.bright_white": "#c4c1c1",
                "modified": "#C54730",
                "syntax": {
                    "attribute": {
                        "color": "#C54730"
                    },
                    "boolean": {
                      "color": "#EC592C"
                    },
                    "comment": {
                      "color": "#6c5858"
                    },
                    "comment.doc": {
                      "color": "#6c5858"
                    },
                    "constant": {
                      "color": "#EC592C"
                    },
                    "function": {
                      "color": "#F6B08D"
                    },
                    "keyword": {
                      "color": "#5F7C83"
                    },
                    "number": {
                      "color": "#6D8D91"
                    },
                    "operator": {
                      "color": "#5F7C83"
                    },
                    "string": {
                      "color": "#C54730"
                    },
                    "variable": {
                      "color": "#EC592C"
                    }
                },
                "players": [
                  {
                    "cursor": "#C54730",
                    "background": "#000000",
                    "selection": "#C5473030"
                  }
                ]
            }
        }
    ]
}
