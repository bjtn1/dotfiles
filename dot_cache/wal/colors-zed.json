{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c7c3c2",
                "text": "#c7c3c2",
                "text.muted": "#c7c3c270",
                "text.ignored": "#c7c3c240",
                "text.placeholder": "#c7c3c250",
                "ignored": "#c7c3c230",
                "element.hover": "#c7c3c230",
                "ghost_element.hover": "#8F725A30",
                "ghost_element.selected": "#8F725A30",
                "ghost_element.active": "#8F725A60",
                "border": "#000000",
                "editor.highlighted_line.background": "#8F725A10",
                "editor.active_line.background": "#8F725A10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#8F725A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#8F725A",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c7c3c2",
                "terminal.dim_foreground": "#c7c3c2",
                "terminal.bright_foreground": "#c7c3c2",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#8F725A",
                "terminal.ansi.green": "#7C8476",
                "terminal.ansi.yellow": "#A1886E",
                "terminal.ansi.blue": "#CDA272",
                "terminal.ansi.magenta": "#A0A49A",
                "terminal.ansi.cyan": "#D0BA98",
                "terminal.ansi.white": "#c7c3c2",
                "terminal.ansi.bright_black": "#71615c",
                "terminal.ansi.bright_red": "#8F725A",
                "terminal.ansi.bright_green": "#7C8476",
                "terminal.ansi.bright_yellow": "#A1886E",
                "terminal.ansi.bright_blue": "#CDA272",
                "terminal.ansi.bright_magenta": "#A0A49A",
                "terminal.ansi.bright_cyan": "#D0BA98",
                "terminal.ansi.bright_white": "#c7c3c2",
                "modified": "#8F725A",
                "syntax": {
                    "attribute": {
                        "color": "#8F725A"
                    },
                    "boolean": {
                      "color": "#7C8476"
                    },
                    "comment": {
                      "color": "#71615c"
                    },
                    "comment.doc": {
                      "color": "#71615c"
                    },
                    "constant": {
                      "color": "#7C8476"
                    },
                    "function": {
                      "color": "#D0BA98"
                    },
                    "keyword": {
                      "color": "#CDA272"
                    },
                    "number": {
                      "color": "#A0A49A"
                    },
                    "operator": {
                      "color": "#CDA272"
                    },
                    "string": {
                      "color": "#8F725A"
                    },
                    "variable": {
                      "color": "#7C8476"
                    }
                },
                "players": [
                  {
                    "cursor": "#8F725A",
                    "background": "#000000",
                    "selection": "#8F725A30"
                  }
                ]
            }
        }
    ]
}
