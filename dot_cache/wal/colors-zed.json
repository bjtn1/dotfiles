{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c1c1",
                "text": "#c3c1c1",
                "text.muted": "#c3c1c170",
                "text.ignored": "#c3c1c140",
                "text.placeholder": "#c3c1c150",
                "ignored": "#c3c1c130",
                "element.hover": "#c3c1c130",
                "ghost_element.hover": "#BB444630",
                "ghost_element.selected": "#BB444630",
                "ghost_element.active": "#BB444660",
                "border": "#000000",
                "editor.highlighted_line.background": "#BB444610",
                "editor.active_line.background": "#BB444610",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#BB444630",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#BB4446",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c1c1",
                "terminal.dim_foreground": "#c3c1c1",
                "terminal.bright_foreground": "#c3c1c1",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#BB4446",
                "terminal.ansi.green": "#EC665B",
                "terminal.ansi.yellow": "#AB8975",
                "terminal.ansi.blue": "#F9946D",
                "terminal.ansi.magenta": "#F75B8F",
                "terminal.ansi.cyan": "#F4A897",
                "terminal.ansi.white": "#c3c1c1",
                "terminal.ansi.bright_black": "#6a5a57",
                "terminal.ansi.bright_red": "#BB4446",
                "terminal.ansi.bright_green": "#EC665B",
                "terminal.ansi.bright_yellow": "#AB8975",
                "terminal.ansi.bright_blue": "#F9946D",
                "terminal.ansi.bright_magenta": "#F75B8F",
                "terminal.ansi.bright_cyan": "#F4A897",
                "terminal.ansi.bright_white": "#c3c1c1",
                "modified": "#BB4446",
                "syntax": {
                    "attribute": {
                        "color": "#BB4446"
                    },
                    "boolean": {
                      "color": "#EC665B"
                    },
                    "comment": {
                      "color": "#6a5a57"
                    },
                    "comment.doc": {
                      "color": "#6a5a57"
                    },
                    "constant": {
                      "color": "#EC665B"
                    },
                    "function": {
                      "color": "#F4A897"
                    },
                    "keyword": {
                      "color": "#F9946D"
                    },
                    "number": {
                      "color": "#F75B8F"
                    },
                    "operator": {
                      "color": "#F9946D"
                    },
                    "string": {
                      "color": "#BB4446"
                    },
                    "variable": {
                      "color": "#EC665B"
                    }
                },
                "players": [
                  {
                    "cursor": "#BB4446",
                    "background": "#000000",
                    "selection": "#BB444630"
                  }
                ]
            }
        }
    ]
}
