{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c8cbcf",
                "text": "#c8cbcf",
                "text.muted": "#c8cbcf70",
                "text.ignored": "#c8cbcf40",
                "text.placeholder": "#c8cbcf50",
                "ignored": "#c8cbcf30",
                "element.hover": "#c8cbcf30",
                "ghost_element.hover": "#71CAE430",
                "ghost_element.selected": "#71CAE430",
                "ghost_element.active": "#71CAE460",
                "border": "#000000",
                "editor.highlighted_line.background": "#71CAE410",
                "editor.active_line.background": "#71CAE410",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#71CAE430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#71CAE4",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c8cbcf",
                "terminal.dim_foreground": "#c8cbcf",
                "terminal.bright_foreground": "#c8cbcf",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#71CAE4",
                "terminal.ansi.green": "#A0A2A7",
                "terminal.ansi.yellow": "#E0B0A5",
                "terminal.ansi.blue": "#F1CFA7",
                "terminal.ansi.magenta": "#BABAC3",
                "terminal.ansi.cyan": "#A2CFE5",
                "terminal.ansi.white": "#c8cbcf",
                "terminal.ansi.bright_black": "#6c7785",
                "terminal.ansi.bright_red": "#71CAE4",
                "terminal.ansi.bright_green": "#A0A2A7",
                "terminal.ansi.bright_yellow": "#E0B0A5",
                "terminal.ansi.bright_blue": "#F1CFA7",
                "terminal.ansi.bright_magenta": "#BABAC3",
                "terminal.ansi.bright_cyan": "#A2CFE5",
                "terminal.ansi.bright_white": "#c8cbcf",
                "modified": "#71CAE4",
                "syntax": {
                    "attribute": {
                        "color": "#71CAE4"
                    },
                    "boolean": {
                      "color": "#A0A2A7"
                    },
                    "comment": {
                      "color": "#6c7785"
                    },
                    "comment.doc": {
                      "color": "#6c7785"
                    },
                    "constant": {
                      "color": "#A0A2A7"
                    },
                    "function": {
                      "color": "#A2CFE5"
                    },
                    "keyword": {
                      "color": "#F1CFA7"
                    },
                    "number": {
                      "color": "#BABAC3"
                    },
                    "operator": {
                      "color": "#F1CFA7"
                    },
                    "string": {
                      "color": "#71CAE4"
                    },
                    "variable": {
                      "color": "#A0A2A7"
                    }
                },
                "players": [
                  {
                    "cursor": "#71CAE4",
                    "background": "#000000",
                    "selection": "#71CAE430"
                  }
                ]
            }
        }
    ]
}
