{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c3c5",
                "text": "#c2c3c5",
                "text.muted": "#c2c3c570",
                "text.ignored": "#c2c3c540",
                "text.placeholder": "#c2c3c550",
                "ignored": "#c2c3c530",
                "element.hover": "#c2c3c530",
                "ghost_element.hover": "#34487130",
                "ghost_element.selected": "#34487130",
                "ghost_element.active": "#34487160",
                "border": "#000000",
                "editor.highlighted_line.background": "#34487110",
                "editor.active_line.background": "#34487110",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#34487130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#344871",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c3c5",
                "terminal.dim_foreground": "#c2c3c5",
                "terminal.bright_foreground": "#c2c3c5",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#344871",
                "terminal.ansi.green": "#4F516E",
                "terminal.ansi.yellow": "#37518C",
                "terminal.ansi.blue": "#4B55A0",
                "terminal.ansi.magenta": "#5468A0",
                "terminal.ansi.cyan": "#906983",
                "terminal.ansi.white": "#c2c3c5",
                "terminal.ansi.bright_black": "#5a636e",
                "terminal.ansi.bright_red": "#344871",
                "terminal.ansi.bright_green": "#4F516E",
                "terminal.ansi.bright_yellow": "#37518C",
                "terminal.ansi.bright_blue": "#4B55A0",
                "terminal.ansi.bright_magenta": "#5468A0",
                "terminal.ansi.bright_cyan": "#906983",
                "terminal.ansi.bright_white": "#c2c3c5",
                "modified": "#344871",
                "syntax": {
                    "attribute": {
                        "color": "#344871"
                    },
                    "boolean": {
                      "color": "#4F516E"
                    },
                    "comment": {
                      "color": "#5a636e"
                    },
                    "comment.doc": {
                      "color": "#5a636e"
                    },
                    "constant": {
                      "color": "#4F516E"
                    },
                    "function": {
                      "color": "#906983"
                    },
                    "keyword": {
                      "color": "#4B55A0"
                    },
                    "number": {
                      "color": "#5468A0"
                    },
                    "operator": {
                      "color": "#4B55A0"
                    },
                    "string": {
                      "color": "#344871"
                    },
                    "variable": {
                      "color": "#4F516E"
                    }
                },
                "players": [
                  {
                    "cursor": "#344871",
                    "background": "#000000",
                    "selection": "#34487130"
                  }
                ]
            }
        }
    ]
}
