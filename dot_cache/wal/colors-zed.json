{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c5c6",
                "text": "#c3c5c6",
                "text.muted": "#c3c5c670",
                "text.ignored": "#c3c5c640",
                "text.placeholder": "#c3c5c650",
                "ignored": "#c3c5c630",
                "element.hover": "#c3c5c630",
                "ghost_element.hover": "#AD916F30",
                "ghost_element.selected": "#AD916F30",
                "ghost_element.active": "#AD916F60",
                "border": "#000000",
                "editor.highlighted_line.background": "#AD916F10",
                "editor.active_line.background": "#AD916F10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#AD916F30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#AD916F",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c5c6",
                "terminal.dim_foreground": "#c3c5c6",
                "terminal.bright_foreground": "#c3c5c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#AD916F",
                "terminal.ansi.green": "#517987",
                "terminal.ansi.yellow": "#8B7E81",
                "terminal.ansi.blue": "#5A8C91",
                "terminal.ansi.magenta": "#A49E97",
                "terminal.ansi.cyan": "#DDAD93",
                "terminal.ansi.white": "#c3c5c6",
                "terminal.ansi.bright_black": "#5d6a71",
                "terminal.ansi.bright_red": "#AD916F",
                "terminal.ansi.bright_green": "#517987",
                "terminal.ansi.bright_yellow": "#8B7E81",
                "terminal.ansi.bright_blue": "#5A8C91",
                "terminal.ansi.bright_magenta": "#A49E97",
                "terminal.ansi.bright_cyan": "#DDAD93",
                "terminal.ansi.bright_white": "#c3c5c6",
                "modified": "#AD916F",
                "syntax": {
                    "attribute": {
                        "color": "#AD916F"
                    },
                    "boolean": {
                      "color": "#517987"
                    },
                    "comment": {
                      "color": "#5d6a71"
                    },
                    "comment.doc": {
                      "color": "#5d6a71"
                    },
                    "constant": {
                      "color": "#517987"
                    },
                    "function": {
                      "color": "#DDAD93"
                    },
                    "keyword": {
                      "color": "#5A8C91"
                    },
                    "number": {
                      "color": "#A49E97"
                    },
                    "operator": {
                      "color": "#5A8C91"
                    },
                    "string": {
                      "color": "#AD916F"
                    },
                    "variable": {
                      "color": "#517987"
                    }
                },
                "players": [
                  {
                    "cursor": "#AD916F",
                    "background": "#000000",
                    "selection": "#AD916F30"
                  }
                ]
            }
        }
    ]
}
