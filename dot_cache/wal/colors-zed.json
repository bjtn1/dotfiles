{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c3c6",
                "text": "#c3c3c6",
                "text.muted": "#c3c3c670",
                "text.ignored": "#c3c3c640",
                "text.placeholder": "#c3c3c650",
                "ignored": "#c3c3c630",
                "element.hover": "#c3c3c630",
                "ghost_element.hover": "#39445C30",
                "ghost_element.selected": "#39445C30",
                "ghost_element.active": "#39445C60",
                "border": "#000000",
                "editor.highlighted_line.background": "#39445C10",
                "editor.active_line.background": "#39445C10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#39445C30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#39445C",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c3c6",
                "terminal.dim_foreground": "#c3c3c6",
                "terminal.bright_foreground": "#c3c3c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#39445C",
                "terminal.ansi.green": "#3E4A63",
                "terminal.ansi.yellow": "#43425E",
                "terminal.ansi.blue": "#4E4A77",
                "terminal.ansi.magenta": "#4A637D",
                "terminal.ansi.cyan": "#555384",
                "terminal.ansi.white": "#c3c3c6",
                "terminal.ansi.bright_black": "#5d5d71",
                "terminal.ansi.bright_red": "#39445C",
                "terminal.ansi.bright_green": "#3E4A63",
                "terminal.ansi.bright_yellow": "#43425E",
                "terminal.ansi.bright_blue": "#4E4A77",
                "terminal.ansi.bright_magenta": "#4A637D",
                "terminal.ansi.bright_cyan": "#555384",
                "terminal.ansi.bright_white": "#c3c3c6",
                "modified": "#39445C",
                "syntax": {
                    "attribute": {
                        "color": "#39445C"
                    },
                    "boolean": {
                      "color": "#3E4A63"
                    },
                    "comment": {
                      "color": "#5d5d71"
                    },
                    "comment.doc": {
                      "color": "#5d5d71"
                    },
                    "constant": {
                      "color": "#3E4A63"
                    },
                    "function": {
                      "color": "#555384"
                    },
                    "keyword": {
                      "color": "#4E4A77"
                    },
                    "number": {
                      "color": "#4A637D"
                    },
                    "operator": {
                      "color": "#4E4A77"
                    },
                    "string": {
                      "color": "#39445C"
                    },
                    "variable": {
                      "color": "#3E4A63"
                    }
                },
                "players": [
                  {
                    "cursor": "#39445C",
                    "background": "#000000",
                    "selection": "#39445C30"
                  }
                ]
            }
        }
    ]
}
