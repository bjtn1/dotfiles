{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c1c0c0",
                "text": "#c1c0c0",
                "text.muted": "#c1c0c070",
                "text.ignored": "#c1c0c040",
                "text.placeholder": "#c1c0c050",
                "ignored": "#c1c0c030",
                "element.hover": "#c1c0c030",
                "ghost_element.hover": "#93729030",
                "ghost_element.selected": "#93729030",
                "ghost_element.active": "#93729060",
                "border": "#000000",
                "editor.highlighted_line.background": "#93729010",
                "editor.active_line.background": "#93729010",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#93729030",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#937290",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c1c0c0",
                "terminal.dim_foreground": "#c1c0c0",
                "terminal.bright_foreground": "#c1c0c0",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#937290",
                "terminal.ansi.green": "#AF78AC",
                "terminal.ansi.yellow": "#90879A",
                "terminal.ansi.blue": "#A290A3",
                "terminal.ansi.magenta": "#AF96A6",
                "terminal.ansi.cyan": "#D4A7AF",
                "terminal.ansi.white": "#c1c0c0",
                "terminal.ansi.bright_black": "#665353",
                "terminal.ansi.bright_red": "#937290",
                "terminal.ansi.bright_green": "#AF78AC",
                "terminal.ansi.bright_yellow": "#90879A",
                "terminal.ansi.bright_blue": "#A290A3",
                "terminal.ansi.bright_magenta": "#AF96A6",
                "terminal.ansi.bright_cyan": "#D4A7AF",
                "terminal.ansi.bright_white": "#c1c0c0",
                "modified": "#937290",
                "syntax": {
                    "attribute": {
                        "color": "#937290"
                    },
                    "boolean": {
                      "color": "#AF78AC"
                    },
                    "comment": {
                      "color": "#665353"
                    },
                    "comment.doc": {
                      "color": "#665353"
                    },
                    "constant": {
                      "color": "#AF78AC"
                    },
                    "function": {
                      "color": "#D4A7AF"
                    },
                    "keyword": {
                      "color": "#A290A3"
                    },
                    "number": {
                      "color": "#AF96A6"
                    },
                    "operator": {
                      "color": "#A290A3"
                    },
                    "string": {
                      "color": "#937290"
                    },
                    "variable": {
                      "color": "#AF78AC"
                    }
                },
                "players": [
                  {
                    "cursor": "#937290",
                    "background": "#000000",
                    "selection": "#93729030"
                  }
                ]
            }
        }
    ]
}
