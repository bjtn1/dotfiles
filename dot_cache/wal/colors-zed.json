{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c8c6ca",
                "text": "#c8c6ca",
                "text.muted": "#c8c6ca70",
                "text.ignored": "#c8c6ca40",
                "text.placeholder": "#c8c6ca50",
                "ignored": "#c8c6ca30",
                "element.hover": "#c8c6ca30",
                "ghost_element.hover": "#274D8E30",
                "ghost_element.selected": "#274D8E30",
                "ghost_element.active": "#274D8E60",
                "border": "#000000",
                "editor.highlighted_line.background": "#274D8E10",
                "editor.active_line.background": "#274D8E10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#274D8E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#274D8E",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c8c6ca",
                "terminal.dim_foreground": "#c8c6ca",
                "terminal.bright_foreground": "#c8c6ca",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#274D8E",
                "terminal.ansi.green": "#2962A9",
                "terminal.ansi.yellow": "#486098",
                "terminal.ansi.blue": "#1F76D4",
                "terminal.ansi.magenta": "#90718B",
                "terminal.ansi.cyan": "#2599DF",
                "terminal.ansi.white": "#c8c6ca",
                "terminal.ansi.bright_black": "#6f657b",
                "terminal.ansi.bright_red": "#274D8E",
                "terminal.ansi.bright_green": "#2962A9",
                "terminal.ansi.bright_yellow": "#486098",
                "terminal.ansi.bright_blue": "#1F76D4",
                "terminal.ansi.bright_magenta": "#90718B",
                "terminal.ansi.bright_cyan": "#2599DF",
                "terminal.ansi.bright_white": "#c8c6ca",
                "modified": "#274D8E",
                "syntax": {
                    "attribute": {
                        "color": "#274D8E"
                    },
                    "boolean": {
                      "color": "#2962A9"
                    },
                    "comment": {
                      "color": "#6f657b"
                    },
                    "comment.doc": {
                      "color": "#6f657b"
                    },
                    "constant": {
                      "color": "#2962A9"
                    },
                    "function": {
                      "color": "#2599DF"
                    },
                    "keyword": {
                      "color": "#1F76D4"
                    },
                    "number": {
                      "color": "#90718B"
                    },
                    "operator": {
                      "color": "#1F76D4"
                    },
                    "string": {
                      "color": "#274D8E"
                    },
                    "variable": {
                      "color": "#2962A9"
                    }
                },
                "players": [
                  {
                    "cursor": "#274D8E",
                    "background": "#000000",
                    "selection": "#274D8E30"
                  }
                ]
            }
        }
    ]
}
