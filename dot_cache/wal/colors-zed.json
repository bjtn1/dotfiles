{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c3c4",
                "text": "#c3c3c4",
                "text.muted": "#c3c3c470",
                "text.ignored": "#c3c3c440",
                "text.placeholder": "#c3c3c450",
                "ignored": "#c3c3c430",
                "element.hover": "#c3c3c430",
                "ghost_element.hover": "#65484730",
                "ghost_element.selected": "#65484730",
                "ghost_element.active": "#65484760",
                "border": "#000000",
                "editor.highlighted_line.background": "#65484710",
                "editor.active_line.background": "#65484710",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#65484730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#654847",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c3c4",
                "terminal.dim_foreground": "#c3c3c4",
                "terminal.bright_foreground": "#c3c3c4",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#654847",
                "terminal.ansi.green": "#90363B",
                "terminal.ansi.yellow": "#913A45",
                "terminal.ansi.blue": "#A0514F",
                "terminal.ansi.magenta": "#D3685B",
                "terminal.ansi.cyan": "#E0956E",
                "terminal.ansi.white": "#c3c3c4",
                "terminal.ansi.bright_black": "#5f5a6f",
                "terminal.ansi.bright_red": "#654847",
                "terminal.ansi.bright_green": "#90363B",
                "terminal.ansi.bright_yellow": "#913A45",
                "terminal.ansi.bright_blue": "#A0514F",
                "terminal.ansi.bright_magenta": "#D3685B",
                "terminal.ansi.bright_cyan": "#E0956E",
                "terminal.ansi.bright_white": "#c3c3c4",
                "modified": "#654847",
                "syntax": {
                    "attribute": {
                        "color": "#654847"
                    },
                    "boolean": {
                      "color": "#90363B"
                    },
                    "comment": {
                      "color": "#5f5a6f"
                    },
                    "comment.doc": {
                      "color": "#5f5a6f"
                    },
                    "constant": {
                      "color": "#90363B"
                    },
                    "function": {
                      "color": "#E0956E"
                    },
                    "keyword": {
                      "color": "#A0514F"
                    },
                    "number": {
                      "color": "#D3685B"
                    },
                    "operator": {
                      "color": "#A0514F"
                    },
                    "string": {
                      "color": "#654847"
                    },
                    "variable": {
                      "color": "#90363B"
                    }
                },
                "players": [
                  {
                    "cursor": "#654847",
                    "background": "#000000",
                    "selection": "#65484730"
                  }
                ]
            }
        }
    ]
}
