{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c8",
                "text": "#c3c4c8",
                "text.muted": "#c3c4c870",
                "text.ignored": "#c3c4c840",
                "text.placeholder": "#c3c4c850",
                "ignored": "#c3c4c830",
                "element.hover": "#c3c4c830",
                "ghost_element.hover": "#274D9030",
                "ghost_element.selected": "#274D9030",
                "ghost_element.active": "#274D9060",
                "border": "#000000",
                "editor.highlighted_line.background": "#274D9010",
                "editor.active_line.background": "#274D9010",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#274D9030",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#274D90",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c8",
                "terminal.dim_foreground": "#c3c4c8",
                "terminal.bright_foreground": "#c3c4c8",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#274D90",
                "terminal.ansi.green": "#255197",
                "terminal.ansi.yellow": "#3469B3",
                "terminal.ansi.blue": "#3A6AB6",
                "terminal.ansi.magenta": "#4868B3",
                "terminal.ansi.cyan": "#3AA0DC",
                "terminal.ansi.white": "#c3c4c8",
                "terminal.ansi.bright_black": "#5f6374",
                "terminal.ansi.bright_red": "#274D90",
                "terminal.ansi.bright_green": "#255197",
                "terminal.ansi.bright_yellow": "#3469B3",
                "terminal.ansi.bright_blue": "#3A6AB6",
                "terminal.ansi.bright_magenta": "#4868B3",
                "terminal.ansi.bright_cyan": "#3AA0DC",
                "terminal.ansi.bright_white": "#c3c4c8",
                "modified": "#274D90",
                "syntax": {
                    "attribute": {
                        "color": "#274D90"
                    },
                    "boolean": {
                      "color": "#255197"
                    },
                    "comment": {
                      "color": "#5f6374"
                    },
                    "comment.doc": {
                      "color": "#5f6374"
                    },
                    "constant": {
                      "color": "#255197"
                    },
                    "function": {
                      "color": "#3AA0DC"
                    },
                    "keyword": {
                      "color": "#3A6AB6"
                    },
                    "number": {
                      "color": "#4868B3"
                    },
                    "operator": {
                      "color": "#3A6AB6"
                    },
                    "string": {
                      "color": "#274D90"
                    },
                    "variable": {
                      "color": "#255197"
                    }
                },
                "players": [
                  {
                    "cursor": "#274D90",
                    "background": "#000000",
                    "selection": "#274D9030"
                  }
                ]
            }
        }
    ]
}
