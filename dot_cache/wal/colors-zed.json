{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c7",
                "text": "#c3c4c7",
                "text.muted": "#c3c4c770",
                "text.ignored": "#c3c4c740",
                "text.placeholder": "#c3c4c750",
                "ignored": "#c3c4c730",
                "element.hover": "#c3c4c730",
                "ghost_element.hover": "#2F6F9130",
                "ghost_element.selected": "#2F6F9130",
                "ghost_element.active": "#2F6F9160",
                "border": "#000000",
                "editor.highlighted_line.background": "#2F6F9110",
                "editor.active_line.background": "#2F6F9110",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#2F6F9130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#2F6F91",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c7",
                "terminal.dim_foreground": "#c3c4c7",
                "terminal.bright_foreground": "#c3c4c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#2F6F91",
                "terminal.ansi.green": "#51778F",
                "terminal.ansi.yellow": "#5B92AA",
                "terminal.ansi.blue": "#4EAAC9",
                "terminal.ansi.magenta": "#5DB0CE",
                "terminal.ansi.cyan": "#BFC9AE",
                "terminal.ansi.white": "#c3c4c7",
                "terminal.ansi.bright_black": "#5d6372",
                "terminal.ansi.bright_red": "#2F6F91",
                "terminal.ansi.bright_green": "#51778F",
                "terminal.ansi.bright_yellow": "#5B92AA",
                "terminal.ansi.bright_blue": "#4EAAC9",
                "terminal.ansi.bright_magenta": "#5DB0CE",
                "terminal.ansi.bright_cyan": "#BFC9AE",
                "terminal.ansi.bright_white": "#c3c4c7",
                "modified": "#2F6F91",
                "syntax": {
                    "attribute": {
                        "color": "#2F6F91"
                    },
                    "boolean": {
                      "color": "#51778F"
                    },
                    "comment": {
                      "color": "#5d6372"
                    },
                    "comment.doc": {
                      "color": "#5d6372"
                    },
                    "constant": {
                      "color": "#51778F"
                    },
                    "function": {
                      "color": "#BFC9AE"
                    },
                    "keyword": {
                      "color": "#4EAAC9"
                    },
                    "number": {
                      "color": "#5DB0CE"
                    },
                    "operator": {
                      "color": "#4EAAC9"
                    },
                    "string": {
                      "color": "#2F6F91"
                    },
                    "variable": {
                      "color": "#51778F"
                    }
                },
                "players": [
                  {
                    "cursor": "#2F6F91",
                    "background": "#000000",
                    "selection": "#2F6F9130"
                  }
                ]
            }
        }
    ]
}
