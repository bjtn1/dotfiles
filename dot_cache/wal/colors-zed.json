{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6c4c6",
                "text": "#c6c4c6",
                "text.muted": "#c6c4c670",
                "text.ignored": "#c6c4c640",
                "text.placeholder": "#c6c4c650",
                "ignored": "#c6c4c630",
                "element.hover": "#c6c4c630",
                "ghost_element.hover": "#B0966D30",
                "ghost_element.selected": "#B0966D30",
                "ghost_element.active": "#B0966D60",
                "border": "#000000",
                "editor.highlighted_line.background": "#B0966D10",
                "editor.active_line.background": "#B0966D10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#B0966D30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#B0966D",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6c4c6",
                "terminal.dim_foreground": "#c6c4c6",
                "terminal.bright_foreground": "#c6c4c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#B0966D",
                "terminal.ansi.green": "#D7996E",
                "terminal.ansi.yellow": "#767886",
                "terminal.ansi.blue": "#A69A95",
                "terminal.ansi.magenta": "#D3B196",
                "terminal.ansi.cyan": "#E9D0A9",
                "terminal.ansi.white": "#c6c4c6",
                "terminal.ansi.bright_black": "#6b5e74",
                "terminal.ansi.bright_red": "#B0966D",
                "terminal.ansi.bright_green": "#D7996E",
                "terminal.ansi.bright_yellow": "#767886",
                "terminal.ansi.bright_blue": "#A69A95",
                "terminal.ansi.bright_magenta": "#D3B196",
                "terminal.ansi.bright_cyan": "#E9D0A9",
                "terminal.ansi.bright_white": "#c6c4c6",
                "modified": "#B0966D",
                "syntax": {
                    "attribute": {
                        "color": "#B0966D"
                    },
                    "boolean": {
                      "color": "#D7996E"
                    },
                    "comment": {
                      "color": "#6b5e74"
                    },
                    "comment.doc": {
                      "color": "#6b5e74"
                    },
                    "constant": {
                      "color": "#D7996E"
                    },
                    "function": {
                      "color": "#E9D0A9"
                    },
                    "keyword": {
                      "color": "#A69A95"
                    },
                    "number": {
                      "color": "#D3B196"
                    },
                    "operator": {
                      "color": "#A69A95"
                    },
                    "string": {
                      "color": "#B0966D"
                    },
                    "variable": {
                      "color": "#D7996E"
                    }
                },
                "players": [
                  {
                    "cursor": "#B0966D",
                    "background": "#000000",
                    "selection": "#B0966D30"
                  }
                ]
            }
        }
    ]
}
