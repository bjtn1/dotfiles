{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6c2c2",
                "text": "#c6c2c2",
                "text.muted": "#c6c2c270",
                "text.ignored": "#c6c2c240",
                "text.placeholder": "#c6c2c250",
                "ignored": "#c6c2c230",
                "element.hover": "#c6c2c230",
                "ghost_element.hover": "#ED6D4B30",
                "ghost_element.selected": "#ED6D4B30",
                "ghost_element.active": "#ED6D4B60",
                "border": "#000000",
                "editor.highlighted_line.background": "#ED6D4B10",
                "editor.active_line.background": "#ED6D4B10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#ED6D4B30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#ED6D4B",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6c2c2",
                "terminal.dim_foreground": "#c6c2c2",
                "terminal.bright_foreground": "#c6c2c2",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#ED6D4B",
                "terminal.ansi.green": "#E6B45F",
                "terminal.ansi.yellow": "#E5A161",
                "terminal.ansi.blue": "#8E7F8D",
                "terminal.ansi.magenta": "#A9A096",
                "terminal.ansi.cyan": "#DFB689",
                "terminal.ansi.white": "#c6c2c2",
                "terminal.ansi.bright_black": "#6f5b5b",
                "terminal.ansi.bright_red": "#ED6D4B",
                "terminal.ansi.bright_green": "#E6B45F",
                "terminal.ansi.bright_yellow": "#E5A161",
                "terminal.ansi.bright_blue": "#8E7F8D",
                "terminal.ansi.bright_magenta": "#A9A096",
                "terminal.ansi.bright_cyan": "#DFB689",
                "terminal.ansi.bright_white": "#c6c2c2",
                "modified": "#ED6D4B",
                "syntax": {
                    "attribute": {
                        "color": "#ED6D4B"
                    },
                    "boolean": {
                      "color": "#E6B45F"
                    },
                    "comment": {
                      "color": "#6f5b5b"
                    },
                    "comment.doc": {
                      "color": "#6f5b5b"
                    },
                    "constant": {
                      "color": "#E6B45F"
                    },
                    "function": {
                      "color": "#DFB689"
                    },
                    "keyword": {
                      "color": "#8E7F8D"
                    },
                    "number": {
                      "color": "#A9A096"
                    },
                    "operator": {
                      "color": "#8E7F8D"
                    },
                    "string": {
                      "color": "#ED6D4B"
                    },
                    "variable": {
                      "color": "#E6B45F"
                    }
                },
                "players": [
                  {
                    "cursor": "#ED6D4B",
                    "background": "#000000",
                    "selection": "#ED6D4B30"
                  }
                ]
            }
        }
    ]
}
