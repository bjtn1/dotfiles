{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c2c7",
                "text": "#c3c2c7",
                "text.muted": "#c3c2c770",
                "text.ignored": "#c3c2c740",
                "text.placeholder": "#c3c2c750",
                "ignored": "#c3c2c730",
                "element.hover": "#c3c2c730",
                "ghost_element.hover": "#57326430",
                "ghost_element.selected": "#57326430",
                "ghost_element.active": "#57326460",
                "border": "#000000",
                "editor.highlighted_line.background": "#57326410",
                "editor.active_line.background": "#57326410",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#57326430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#573264",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c2c7",
                "terminal.dim_foreground": "#c3c2c7",
                "terminal.bright_foreground": "#c3c2c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#573264",
                "terminal.ansi.green": "#684C68",
                "terminal.ansi.yellow": "#B55D5F",
                "terminal.ansi.blue": "#975B6A",
                "terminal.ansi.magenta": "#E6986F",
                "terminal.ansi.cyan": "#735787",
                "terminal.ansi.white": "#c3c2c7",
                "terminal.ansi.bright_black": "#615d72",
                "terminal.ansi.bright_red": "#573264",
                "terminal.ansi.bright_green": "#684C68",
                "terminal.ansi.bright_yellow": "#B55D5F",
                "terminal.ansi.bright_blue": "#975B6A",
                "terminal.ansi.bright_magenta": "#E6986F",
                "terminal.ansi.bright_cyan": "#735787",
                "terminal.ansi.bright_white": "#c3c2c7",
                "modified": "#573264",
                "syntax": {
                    "attribute": {
                        "color": "#573264"
                    },
                    "boolean": {
                      "color": "#684C68"
                    },
                    "comment": {
                      "color": "#615d72"
                    },
                    "comment.doc": {
                      "color": "#615d72"
                    },
                    "constant": {
                      "color": "#684C68"
                    },
                    "function": {
                      "color": "#735787"
                    },
                    "keyword": {
                      "color": "#975B6A"
                    },
                    "number": {
                      "color": "#E6986F"
                    },
                    "operator": {
                      "color": "#975B6A"
                    },
                    "string": {
                      "color": "#573264"
                    },
                    "variable": {
                      "color": "#684C68"
                    }
                },
                "players": [
                  {
                    "cursor": "#573264",
                    "background": "#000000",
                    "selection": "#57326430"
                  }
                ]
            }
        }
    ]
}
