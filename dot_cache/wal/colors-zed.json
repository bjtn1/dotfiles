{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6cfc8",
                "text": "#c6cfc8",
                "text.muted": "#c6cfc870",
                "text.ignored": "#c6cfc840",
                "text.placeholder": "#c6cfc850",
                "ignored": "#c6cfc830",
                "element.hover": "#c6cfc830",
                "ghost_element.hover": "#506F9330",
                "ghost_element.selected": "#506F9330",
                "ghost_element.active": "#506F9360",
                "border": "#000000",
                "editor.highlighted_line.background": "#506F9310",
                "editor.active_line.background": "#506F9310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#506F9330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#506F93",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6cfc8",
                "terminal.dim_foreground": "#c6cfc8",
                "terminal.bright_foreground": "#c6cfc8",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#506F93",
                "terminal.ansi.green": "#A66886",
                "terminal.ansi.yellow": "#619BA0",
                "terminal.ansi.blue": "#49ACB7",
                "terminal.ansi.magenta": "#5FAADA",
                "terminal.ansi.cyan": "#97A69F",
                "terminal.ansi.white": "#c6cfc8",
                "terminal.ansi.bright_black": "#6b826f",
                "terminal.ansi.bright_red": "#506F93",
                "terminal.ansi.bright_green": "#A66886",
                "terminal.ansi.bright_yellow": "#619BA0",
                "terminal.ansi.bright_blue": "#49ACB7",
                "terminal.ansi.bright_magenta": "#5FAADA",
                "terminal.ansi.bright_cyan": "#97A69F",
                "terminal.ansi.bright_white": "#c6cfc8",
                "modified": "#506F93",
                "syntax": {
                    "attribute": {
                        "color": "#506F93"
                    },
                    "boolean": {
                      "color": "#A66886"
                    },
                    "comment": {
                      "color": "#6b826f"
                    },
                    "comment.doc": {
                      "color": "#6b826f"
                    },
                    "constant": {
                      "color": "#A66886"
                    },
                    "function": {
                      "color": "#97A69F"
                    },
                    "keyword": {
                      "color": "#49ACB7"
                    },
                    "number": {
                      "color": "#5FAADA"
                    },
                    "operator": {
                      "color": "#49ACB7"
                    },
                    "string": {
                      "color": "#506F93"
                    },
                    "variable": {
                      "color": "#A66886"
                    }
                },
                "players": [
                  {
                    "cursor": "#506F93",
                    "background": "#000000",
                    "selection": "#506F9330"
                  }
                ]
            }
        }
    ]
}
