{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c4c6",
                "text": "#c2c4c6",
                "text.muted": "#c2c4c670",
                "text.ignored": "#c2c4c640",
                "text.placeholder": "#c2c4c650",
                "ignored": "#c2c4c630",
                "element.hover": "#c2c4c630",
                "ghost_element.hover": "#61545330",
                "ghost_element.selected": "#61545330",
                "ghost_element.active": "#61545360",
                "border": "#000000",
                "editor.highlighted_line.background": "#61545310",
                "editor.active_line.background": "#61545310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#61545330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#615453",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c4c6",
                "terminal.dim_foreground": "#c2c4c6",
                "terminal.bright_foreground": "#c2c4c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#615453",
                "terminal.ansi.green": "#A25836",
                "terminal.ansi.yellow": "#9E6B4F",
                "terminal.ansi.blue": "#D9A058",
                "terminal.ansi.magenta": "#D6B760",
                "terminal.ansi.cyan": "#7A7581",
                "terminal.ansi.white": "#c2c4c6",
                "terminal.ansi.bright_black": "#5c6370",
                "terminal.ansi.bright_red": "#615453",
                "terminal.ansi.bright_green": "#A25836",
                "terminal.ansi.bright_yellow": "#9E6B4F",
                "terminal.ansi.bright_blue": "#D9A058",
                "terminal.ansi.bright_magenta": "#D6B760",
                "terminal.ansi.bright_cyan": "#7A7581",
                "terminal.ansi.bright_white": "#c2c4c6",
                "modified": "#615453",
                "syntax": {
                    "attribute": {
                        "color": "#615453"
                    },
                    "boolean": {
                      "color": "#A25836"
                    },
                    "comment": {
                      "color": "#5c6370"
                    },
                    "comment.doc": {
                      "color": "#5c6370"
                    },
                    "constant": {
                      "color": "#A25836"
                    },
                    "function": {
                      "color": "#7A7581"
                    },
                    "keyword": {
                      "color": "#D9A058"
                    },
                    "number": {
                      "color": "#D6B760"
                    },
                    "operator": {
                      "color": "#D9A058"
                    },
                    "string": {
                      "color": "#615453"
                    },
                    "variable": {
                      "color": "#A25836"
                    }
                },
                "players": [
                  {
                    "cursor": "#615453",
                    "background": "#000000",
                    "selection": "#61545330"
                  }
                ]
            }
        }
    ]
}
