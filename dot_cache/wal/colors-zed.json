{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c9",
                "text": "#c3c4c9",
                "text.muted": "#c3c4c970",
                "text.ignored": "#c3c4c940",
                "text.placeholder": "#c3c4c950",
                "ignored": "#c3c4c930",
                "element.hover": "#c3c4c930",
                "ghost_element.hover": "#667BC330",
                "ghost_element.selected": "#667BC330",
                "ghost_element.active": "#667BC360",
                "border": "#000000",
                "editor.highlighted_line.background": "#667BC310",
                "editor.active_line.background": "#667BC310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#667BC330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#667BC3",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c9",
                "terminal.dim_foreground": "#c3c4c9",
                "terminal.bright_foreground": "#c3c4c9",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#667BC3",
                "terminal.ansi.green": "#9470A7",
                "terminal.ansi.yellow": "#7A83B5",
                "terminal.ansi.blue": "#7388D1",
                "terminal.ansi.magenta": "#7E92E1",
                "terminal.ansi.cyan": "#D1A0BB",
                "terminal.ansi.white": "#c3c4c9",
                "terminal.ansi.bright_black": "#606276",
                "terminal.ansi.bright_red": "#667BC3",
                "terminal.ansi.bright_green": "#9470A7",
                "terminal.ansi.bright_yellow": "#7A83B5",
                "terminal.ansi.bright_blue": "#7388D1",
                "terminal.ansi.bright_magenta": "#7E92E1",
                "terminal.ansi.bright_cyan": "#D1A0BB",
                "terminal.ansi.bright_white": "#c3c4c9",
                "modified": "#667BC3",
                "syntax": {
                    "attribute": {
                        "color": "#667BC3"
                    },
                    "boolean": {
                      "color": "#9470A7"
                    },
                    "comment": {
                      "color": "#606276"
                    },
                    "comment.doc": {
                      "color": "#606276"
                    },
                    "constant": {
                      "color": "#9470A7"
                    },
                    "function": {
                      "color": "#D1A0BB"
                    },
                    "keyword": {
                      "color": "#7388D1"
                    },
                    "number": {
                      "color": "#7E92E1"
                    },
                    "operator": {
                      "color": "#7388D1"
                    },
                    "string": {
                      "color": "#667BC3"
                    },
                    "variable": {
                      "color": "#9470A7"
                    }
                },
                "players": [
                  {
                    "cursor": "#667BC3",
                    "background": "#000000",
                    "selection": "#667BC330"
                  }
                ]
            }
        }
    ]
}
