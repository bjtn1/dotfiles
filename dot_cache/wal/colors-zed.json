{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#cacbcf",
                "text": "#cacbcf",
                "text.muted": "#cacbcf70",
                "text.ignored": "#cacbcf40",
                "text.placeholder": "#cacbcf50",
                "ignored": "#cacbcf30",
                "element.hover": "#cacbcf30",
                "ghost_element.hover": "#3496C530",
                "ghost_element.selected": "#3496C530",
                "ghost_element.active": "#3496C560",
                "border": "#000000",
                "editor.highlighted_line.background": "#3496C510",
                "editor.active_line.background": "#3496C510",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#3496C530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#3496C5",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#cacbcf",
                "terminal.dim_foreground": "#cacbcf",
                "terminal.bright_foreground": "#cacbcf",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#3496C5",
                "terminal.ansi.green": "#17ADD2",
                "terminal.ansi.yellow": "#2CAFCF",
                "terminal.ansi.blue": "#4CB5CD",
                "terminal.ansi.magenta": "#EA9194",
                "terminal.ansi.cyan": "#E8A69F",
                "terminal.ansi.white": "#cacbcf",
                "terminal.ansi.bright_black": "#6f7687",
                "terminal.ansi.bright_red": "#3496C5",
                "terminal.ansi.bright_green": "#17ADD2",
                "terminal.ansi.bright_yellow": "#2CAFCF",
                "terminal.ansi.bright_blue": "#4CB5CD",
                "terminal.ansi.bright_magenta": "#EA9194",
                "terminal.ansi.bright_cyan": "#E8A69F",
                "terminal.ansi.bright_white": "#cacbcf",
                "modified": "#3496C5",
                "syntax": {
                    "attribute": {
                        "color": "#3496C5"
                    },
                    "boolean": {
                      "color": "#17ADD2"
                    },
                    "comment": {
                      "color": "#6f7687"
                    },
                    "comment.doc": {
                      "color": "#6f7687"
                    },
                    "constant": {
                      "color": "#17ADD2"
                    },
                    "function": {
                      "color": "#E8A69F"
                    },
                    "keyword": {
                      "color": "#4CB5CD"
                    },
                    "number": {
                      "color": "#EA9194"
                    },
                    "operator": {
                      "color": "#4CB5CD"
                    },
                    "string": {
                      "color": "#3496C5"
                    },
                    "variable": {
                      "color": "#17ADD2"
                    }
                },
                "players": [
                  {
                    "cursor": "#3496C5",
                    "background": "#000000",
                    "selection": "#3496C530"
                  }
                ]
            }
        }
    ]
}
