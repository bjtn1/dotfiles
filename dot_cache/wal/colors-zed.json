{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c6c2c2",
                "text": "#c6c2c2",
                "text.muted": "#c6c2c270",
                "text.ignored": "#c6c2c240",
                "text.placeholder": "#c6c2c250",
                "ignored": "#c6c2c230",
                "element.hover": "#c6c2c230",
                "ghost_element.hover": "#E05E2130",
                "ghost_element.selected": "#E05E2130",
                "ghost_element.active": "#E05E2160",
                "border": "#000000",
                "editor.highlighted_line.background": "#E05E2110",
                "editor.active_line.background": "#E05E2110",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#E05E2130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#E05E21",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c6c2c2",
                "terminal.dim_foreground": "#c6c2c2",
                "terminal.bright_foreground": "#c6c2c2",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#E05E21",
                "terminal.ansi.green": "#D26F24",
                "terminal.ansi.yellow": "#EA952A",
                "terminal.ansi.blue": "#FDDD27",
                "terminal.ansi.magenta": "#F1A557",
                "terminal.ansi.cyan": "#FAE059",
                "terminal.ansi.white": "#c6c2c2",
                "terminal.ansi.bright_black": "#6f5a5a",
                "terminal.ansi.bright_red": "#E05E21",
                "terminal.ansi.bright_green": "#D26F24",
                "terminal.ansi.bright_yellow": "#EA952A",
                "terminal.ansi.bright_blue": "#FDDD27",
                "terminal.ansi.bright_magenta": "#F1A557",
                "terminal.ansi.bright_cyan": "#FAE059",
                "terminal.ansi.bright_white": "#c6c2c2",
                "modified": "#E05E21",
                "syntax": {
                    "attribute": {
                        "color": "#E05E21"
                    },
                    "boolean": {
                      "color": "#D26F24"
                    },
                    "comment": {
                      "color": "#6f5a5a"
                    },
                    "comment.doc": {
                      "color": "#6f5a5a"
                    },
                    "constant": {
                      "color": "#D26F24"
                    },
                    "function": {
                      "color": "#FAE059"
                    },
                    "keyword": {
                      "color": "#FDDD27"
                    },
                    "number": {
                      "color": "#F1A557"
                    },
                    "operator": {
                      "color": "#FDDD27"
                    },
                    "string": {
                      "color": "#E05E21"
                    },
                    "variable": {
                      "color": "#D26F24"
                    }
                },
                "players": [
                  {
                    "cursor": "#E05E21",
                    "background": "#000000",
                    "selection": "#E05E2130"
                  }
                ]
            }
        }
    ]
}
