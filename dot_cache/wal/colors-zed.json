{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c5c6c7",
                "text": "#c5c6c7",
                "text.muted": "#c5c6c770",
                "text.ignored": "#c5c6c740",
                "text.placeholder": "#c5c6c750",
                "ignored": "#c5c6c730",
                "element.hover": "#c5c6c730",
                "ghost_element.hover": "#50729830",
                "ghost_element.selected": "#50729830",
                "ghost_element.active": "#50729860",
                "border": "#000000",
                "editor.highlighted_line.background": "#50729810",
                "editor.active_line.background": "#50729810",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#50729830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#507298",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c5c6c7",
                "terminal.dim_foreground": "#c5c6c7",
                "terminal.bright_foreground": "#c5c6c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#507298",
                "terminal.ansi.green": "#5D7498",
                "terminal.ansi.yellow": "#6C7792",
                "terminal.ansi.blue": "#6C889A",
                "terminal.ansi.magenta": "#6D97A4",
                "terminal.ansi.cyan": "#7AA8C3",
                "terminal.ansi.white": "#c5c6c7",
                "terminal.ansi.bright_black": "#606676",
                "terminal.ansi.bright_red": "#507298",
                "terminal.ansi.bright_green": "#5D7498",
                "terminal.ansi.bright_yellow": "#6C7792",
                "terminal.ansi.bright_blue": "#6C889A",
                "terminal.ansi.bright_magenta": "#6D97A4",
                "terminal.ansi.bright_cyan": "#7AA8C3",
                "terminal.ansi.bright_white": "#c5c6c7",
                "modified": "#507298",
                "syntax": {
                    "attribute": {
                        "color": "#507298"
                    },
                    "boolean": {
                      "color": "#5D7498"
                    },
                    "comment": {
                      "color": "#606676"
                    },
                    "comment.doc": {
                      "color": "#606676"
                    },
                    "constant": {
                      "color": "#5D7498"
                    },
                    "function": {
                      "color": "#7AA8C3"
                    },
                    "keyword": {
                      "color": "#6C889A"
                    },
                    "number": {
                      "color": "#6D97A4"
                    },
                    "operator": {
                      "color": "#6C889A"
                    },
                    "string": {
                      "color": "#507298"
                    },
                    "variable": {
                      "color": "#5D7498"
                    }
                },
                "players": [
                  {
                    "cursor": "#507298",
                    "background": "#000000",
                    "selection": "#50729830"
                  }
                ]
            }
        }
    ]
}
