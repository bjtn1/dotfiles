{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c2c7",
                "text": "#c4c2c7",
                "text.muted": "#c4c2c770",
                "text.ignored": "#c4c2c740",
                "text.placeholder": "#c4c2c750",
                "ignored": "#c4c2c730",
                "element.hover": "#c4c2c730",
                "ghost_element.hover": "#5B4C7030",
                "ghost_element.selected": "#5B4C7030",
                "ghost_element.active": "#5B4C7060",
                "border": "#000000",
                "editor.highlighted_line.background": "#5B4C7010",
                "editor.active_line.background": "#5B4C7010",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#5B4C7030",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#5B4C70",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c2c7",
                "terminal.dim_foreground": "#c4c2c7",
                "terminal.bright_foreground": "#c4c2c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#5B4C70",
                "terminal.ansi.green": "#603889",
                "terminal.ansi.yellow": "#334DAD",
                "terminal.ansi.blue": "#664D96",
                "terminal.ansi.magenta": "#91438E",
                "terminal.ansi.cyan": "#A0489A",
                "terminal.ansi.white": "#c4c2c7",
                "terminal.ansi.bright_black": "#645e72",
                "terminal.ansi.bright_red": "#5B4C70",
                "terminal.ansi.bright_green": "#603889",
                "terminal.ansi.bright_yellow": "#334DAD",
                "terminal.ansi.bright_blue": "#664D96",
                "terminal.ansi.bright_magenta": "#91438E",
                "terminal.ansi.bright_cyan": "#A0489A",
                "terminal.ansi.bright_white": "#c4c2c7",
                "modified": "#5B4C70",
                "syntax": {
                    "attribute": {
                        "color": "#5B4C70"
                    },
                    "boolean": {
                      "color": "#603889"
                    },
                    "comment": {
                      "color": "#645e72"
                    },
                    "comment.doc": {
                      "color": "#645e72"
                    },
                    "constant": {
                      "color": "#603889"
                    },
                    "function": {
                      "color": "#A0489A"
                    },
                    "keyword": {
                      "color": "#664D96"
                    },
                    "number": {
                      "color": "#91438E"
                    },
                    "operator": {
                      "color": "#664D96"
                    },
                    "string": {
                      "color": "#5B4C70"
                    },
                    "variable": {
                      "color": "#603889"
                    }
                },
                "players": [
                  {
                    "cursor": "#5B4C70",
                    "background": "#000000",
                    "selection": "#5B4C7030"
                  }
                ]
            }
        }
    ]
}
