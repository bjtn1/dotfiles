{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c1c5c5",
                "text": "#c1c5c5",
                "text.muted": "#c1c5c570",
                "text.ignored": "#c1c5c540",
                "text.placeholder": "#c1c5c550",
                "ignored": "#c1c5c530",
                "element.hover": "#c1c5c530",
                "ghost_element.hover": "#31454F30",
                "ghost_element.selected": "#31454F30",
                "ghost_element.active": "#31454F60",
                "border": "#000000",
                "editor.highlighted_line.background": "#31454F10",
                "editor.active_line.background": "#31454F10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#31454F30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#31454F",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c1c5c5",
                "terminal.dim_foreground": "#c1c5c5",
                "terminal.bright_foreground": "#c1c5c5",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#31454F",
                "terminal.ansi.green": "#2B5366",
                "terminal.ansi.yellow": "#4D4F4E",
                "terminal.ansi.blue": "#656766",
                "terminal.ansi.magenta": "#A73B36",
                "terminal.ansi.cyan": "#80817F",
                "terminal.ansi.white": "#c1c5c5",
                "terminal.ansi.bright_black": "#596d6d",
                "terminal.ansi.bright_red": "#31454F",
                "terminal.ansi.bright_green": "#2B5366",
                "terminal.ansi.bright_yellow": "#4D4F4E",
                "terminal.ansi.bright_blue": "#656766",
                "terminal.ansi.bright_magenta": "#A73B36",
                "terminal.ansi.bright_cyan": "#80817F",
                "terminal.ansi.bright_white": "#c1c5c5",
                "modified": "#31454F",
                "syntax": {
                    "attribute": {
                        "color": "#31454F"
                    },
                    "boolean": {
                      "color": "#2B5366"
                    },
                    "comment": {
                      "color": "#596d6d"
                    },
                    "comment.doc": {
                      "color": "#596d6d"
                    },
                    "constant": {
                      "color": "#2B5366"
                    },
                    "function": {
                      "color": "#80817F"
                    },
                    "keyword": {
                      "color": "#656766"
                    },
                    "number": {
                      "color": "#A73B36"
                    },
                    "operator": {
                      "color": "#656766"
                    },
                    "string": {
                      "color": "#31454F"
                    },
                    "variable": {
                      "color": "#2B5366"
                    }
                },
                "players": [
                  {
                    "cursor": "#31454F",
                    "background": "#000000",
                    "selection": "#31454F30"
                  }
                ]
            }
        }
    ]
}
