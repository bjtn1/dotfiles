{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c8cacc",
                "text": "#c8cacc",
                "text.muted": "#c8cacc70",
                "text.ignored": "#c8cacc40",
                "text.placeholder": "#c8cacc50",
                "ignored": "#c8cacc30",
                "element.hover": "#c8cacc30",
                "ghost_element.hover": "#93A0AF30",
                "ghost_element.selected": "#93A0AF30",
                "ghost_element.active": "#93A0AF60",
                "border": "#000000",
                "editor.highlighted_line.background": "#93A0AF10",
                "editor.active_line.background": "#93A0AF10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#93A0AF30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#93A0AF",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c8cacc",
                "terminal.dim_foreground": "#c8cacc",
                "terminal.bright_foreground": "#c8cacc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#93A0AF",
                "terminal.ansi.green": "#CCBBBF",
                "terminal.ansi.yellow": "#9EB7C3",
                "terminal.ansi.blue": "#A8BCC6",
                "terminal.ansi.magenta": "#EEB9CC",
                "terminal.ansi.cyan": "#CAD3DD",
                "terminal.ansi.white": "#c8cacc",
                "terminal.ansi.bright_black": "#6a7382",
                "terminal.ansi.bright_red": "#93A0AF",
                "terminal.ansi.bright_green": "#CCBBBF",
                "terminal.ansi.bright_yellow": "#9EB7C3",
                "terminal.ansi.bright_blue": "#A8BCC6",
                "terminal.ansi.bright_magenta": "#EEB9CC",
                "terminal.ansi.bright_cyan": "#CAD3DD",
                "terminal.ansi.bright_white": "#c8cacc",
                "modified": "#93A0AF",
                "syntax": {
                    "attribute": {
                        "color": "#93A0AF"
                    },
                    "boolean": {
                      "color": "#CCBBBF"
                    },
                    "comment": {
                      "color": "#6a7382"
                    },
                    "comment.doc": {
                      "color": "#6a7382"
                    },
                    "constant": {
                      "color": "#CCBBBF"
                    },
                    "function": {
                      "color": "#CAD3DD"
                    },
                    "keyword": {
                      "color": "#A8BCC6"
                    },
                    "number": {
                      "color": "#EEB9CC"
                    },
                    "operator": {
                      "color": "#A8BCC6"
                    },
                    "string": {
                      "color": "#93A0AF"
                    },
                    "variable": {
                      "color": "#CCBBBF"
                    }
                },
                "players": [
                  {
                    "cursor": "#93A0AF",
                    "background": "#000000",
                    "selection": "#93A0AF30"
                  }
                ]
            }
        }
    ]
}
