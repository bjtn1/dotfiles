{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c4c7",
                "text": "#c2c4c7",
                "text.muted": "#c2c4c770",
                "text.ignored": "#c2c4c740",
                "text.placeholder": "#c2c4c750",
                "ignored": "#c2c4c730",
                "element.hover": "#c2c4c730",
                "ghost_element.hover": "#48515730",
                "ghost_element.selected": "#48515730",
                "ghost_element.active": "#48515760",
                "border": "#000000",
                "editor.highlighted_line.background": "#48515710",
                "editor.active_line.background": "#48515710",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#48515730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#485157",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c4c7",
                "terminal.dim_foreground": "#c2c4c7",
                "terminal.bright_foreground": "#c2c4c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#485157",
                "terminal.ansi.green": "#58646B",
                "terminal.ansi.yellow": "#9E6526",
                "terminal.ansi.blue": "#6B9F73",
                "terminal.ansi.magenta": "#D3AB3E",
                "terminal.ansi.cyan": "#697A85",
                "terminal.ansi.white": "#c2c4c7",
                "terminal.ansi.bright_black": "#5c6571",
                "terminal.ansi.bright_red": "#485157",
                "terminal.ansi.bright_green": "#58646B",
                "terminal.ansi.bright_yellow": "#9E6526",
                "terminal.ansi.bright_blue": "#6B9F73",
                "terminal.ansi.bright_magenta": "#D3AB3E",
                "terminal.ansi.bright_cyan": "#697A85",
                "terminal.ansi.bright_white": "#c2c4c7",
                "modified": "#485157",
                "syntax": {
                    "attribute": {
                        "color": "#485157"
                    },
                    "boolean": {
                      "color": "#58646B"
                    },
                    "comment": {
                      "color": "#5c6571"
                    },
                    "comment.doc": {
                      "color": "#5c6571"
                    },
                    "constant": {
                      "color": "#58646B"
                    },
                    "function": {
                      "color": "#697A85"
                    },
                    "keyword": {
                      "color": "#6B9F73"
                    },
                    "number": {
                      "color": "#D3AB3E"
                    },
                    "operator": {
                      "color": "#6B9F73"
                    },
                    "string": {
                      "color": "#485157"
                    },
                    "variable": {
                      "color": "#58646B"
                    }
                },
                "players": [
                  {
                    "cursor": "#485157",
                    "background": "#000000",
                    "selection": "#48515730"
                  }
                ]
            }
        }
    ]
}
