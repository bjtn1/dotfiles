{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c7",
                "text": "#c3c4c7",
                "text.muted": "#c3c4c770",
                "text.ignored": "#c3c4c740",
                "text.placeholder": "#c3c4c750",
                "ignored": "#c3c4c730",
                "element.hover": "#c3c4c730",
                "ghost_element.hover": "#4C4C5630",
                "ghost_element.selected": "#4C4C5630",
                "ghost_element.active": "#4C4C5660",
                "border": "#000000",
                "editor.highlighted_line.background": "#4C4C5610",
                "editor.active_line.background": "#4C4C5610",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#4C4C5630",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#4C4C56",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c7",
                "terminal.dim_foreground": "#c3c4c7",
                "terminal.bright_foreground": "#c3c4c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#4C4C56",
                "terminal.ansi.green": "#5C5D6A",
                "terminal.ansi.yellow": "#936F6E",
                "terminal.ansi.blue": "#A6867A",
                "terminal.ansi.magenta": "#335D84",
                "terminal.ansi.cyan": "#556E90",
                "terminal.ansi.white": "#c3c4c7",
                "terminal.ansi.bright_black": "#5e6572",
                "terminal.ansi.bright_red": "#4C4C56",
                "terminal.ansi.bright_green": "#5C5D6A",
                "terminal.ansi.bright_yellow": "#936F6E",
                "terminal.ansi.bright_blue": "#A6867A",
                "terminal.ansi.bright_magenta": "#335D84",
                "terminal.ansi.bright_cyan": "#556E90",
                "terminal.ansi.bright_white": "#c3c4c7",
                "modified": "#4C4C56",
                "syntax": {
                    "attribute": {
                        "color": "#4C4C56"
                    },
                    "boolean": {
                      "color": "#5C5D6A"
                    },
                    "comment": {
                      "color": "#5e6572"
                    },
                    "comment.doc": {
                      "color": "#5e6572"
                    },
                    "constant": {
                      "color": "#5C5D6A"
                    },
                    "function": {
                      "color": "#556E90"
                    },
                    "keyword": {
                      "color": "#A6867A"
                    },
                    "number": {
                      "color": "#335D84"
                    },
                    "operator": {
                      "color": "#A6867A"
                    },
                    "string": {
                      "color": "#4C4C56"
                    },
                    "variable": {
                      "color": "#5C5D6A"
                    }
                },
                "players": [
                  {
                    "cursor": "#4C4C56",
                    "background": "#000000",
                    "selection": "#4C4C5630"
                  }
                ]
            }
        }
    ]
}
