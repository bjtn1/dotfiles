{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c2c3c6",
                "text": "#c2c3c6",
                "text.muted": "#c2c3c670",
                "text.ignored": "#c2c3c640",
                "text.placeholder": "#c2c3c650",
                "ignored": "#c2c3c630",
                "element.hover": "#c2c3c630",
                "ghost_element.hover": "#64648730",
                "ghost_element.selected": "#64648730",
                "ghost_element.active": "#64648760",
                "border": "#000000",
                "editor.highlighted_line.background": "#64648710",
                "editor.active_line.background": "#64648710",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#64648730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#646487",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c2c3c6",
                "terminal.dim_foreground": "#c2c3c6",
                "terminal.bright_foreground": "#c2c3c6",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#646487",
                "terminal.ansi.green": "#8F768B",
                "terminal.ansi.yellow": "#837796",
                "terminal.ansi.blue": "#C2A5A6",
                "terminal.ansi.magenta": "#DCACA9",
                "terminal.ansi.cyan": "#ECD7CF",
                "terminal.ansi.white": "#c2c3c6",
                "terminal.ansi.bright_black": "#5a606f",
                "terminal.ansi.bright_red": "#646487",
                "terminal.ansi.bright_green": "#8F768B",
                "terminal.ansi.bright_yellow": "#837796",
                "terminal.ansi.bright_blue": "#C2A5A6",
                "terminal.ansi.bright_magenta": "#DCACA9",
                "terminal.ansi.bright_cyan": "#ECD7CF",
                "terminal.ansi.bright_white": "#c2c3c6",
                "modified": "#646487",
                "syntax": {
                    "attribute": {
                        "color": "#646487"
                    },
                    "boolean": {
                      "color": "#8F768B"
                    },
                    "comment": {
                      "color": "#5a606f"
                    },
                    "comment.doc": {
                      "color": "#5a606f"
                    },
                    "constant": {
                      "color": "#8F768B"
                    },
                    "function": {
                      "color": "#ECD7CF"
                    },
                    "keyword": {
                      "color": "#C2A5A6"
                    },
                    "number": {
                      "color": "#DCACA9"
                    },
                    "operator": {
                      "color": "#C2A5A6"
                    },
                    "string": {
                      "color": "#646487"
                    },
                    "variable": {
                      "color": "#8F768B"
                    }
                },
                "players": [
                  {
                    "cursor": "#646487",
                    "background": "#000000",
                    "selection": "#64648730"
                  }
                ]
            }
        }
    ]
}
