{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c3c4",
                "text": "#c4c3c4",
                "text.muted": "#c4c3c470",
                "text.ignored": "#c4c3c440",
                "text.placeholder": "#c4c3c450",
                "ignored": "#c4c3c430",
                "element.hover": "#c4c3c430",
                "ghost_element.hover": "#B3847830",
                "ghost_element.selected": "#B3847830",
                "ghost_element.active": "#B3847860",
                "border": "#000000",
                "editor.highlighted_line.background": "#B3847810",
                "editor.active_line.background": "#B3847810",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#B3847830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#B38478",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c3c4",
                "terminal.dim_foreground": "#c4c3c4",
                "terminal.bright_foreground": "#c4c3c4",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#B38478",
                "terminal.ansi.green": "#DE9574",
                "terminal.ansi.yellow": "#FDC578",
                "terminal.ansi.blue": "#997F81",
                "terminal.ansi.magenta": "#B79489",
                "terminal.ansi.cyan": "#DFAB90",
                "terminal.ansi.white": "#c4c3c4",
                "terminal.ansi.bright_black": "#6e5a6e",
                "terminal.ansi.bright_red": "#B38478",
                "terminal.ansi.bright_green": "#DE9574",
                "terminal.ansi.bright_yellow": "#FDC578",
                "terminal.ansi.bright_blue": "#997F81",
                "terminal.ansi.bright_magenta": "#B79489",
                "terminal.ansi.bright_cyan": "#DFAB90",
                "terminal.ansi.bright_white": "#c4c3c4",
                "modified": "#B38478",
                "syntax": {
                    "attribute": {
                        "color": "#B38478"
                    },
                    "boolean": {
                      "color": "#DE9574"
                    },
                    "comment": {
                      "color": "#6e5a6e"
                    },
                    "comment.doc": {
                      "color": "#6e5a6e"
                    },
                    "constant": {
                      "color": "#DE9574"
                    },
                    "function": {
                      "color": "#DFAB90"
                    },
                    "keyword": {
                      "color": "#997F81"
                    },
                    "number": {
                      "color": "#B79489"
                    },
                    "operator": {
                      "color": "#997F81"
                    },
                    "string": {
                      "color": "#B38478"
                    },
                    "variable": {
                      "color": "#DE9574"
                    }
                },
                "players": [
                  {
                    "cursor": "#B38478",
                    "background": "#000000",
                    "selection": "#B3847830"
                  }
                ]
            }
        }
    ]
}
