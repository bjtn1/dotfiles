{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c6cc",
                "text": "#c4c6cc",
                "text.muted": "#c4c6cc70",
                "text.ignored": "#c4c6cc40",
                "text.placeholder": "#c4c6cc50",
                "ignored": "#c4c6cc30",
                "element.hover": "#c4c6cc30",
                "ghost_element.hover": "#8D6A8330",
                "ghost_element.selected": "#8D6A8330",
                "ghost_element.active": "#8D6A8360",
                "border": "#000000",
                "editor.highlighted_line.background": "#8D6A8310",
                "editor.active_line.background": "#8D6A8310",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#8D6A8330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#8D6A83",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c6cc",
                "terminal.dim_foreground": "#c4c6cc",
                "terminal.bright_foreground": "#c4c6cc",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#8D6A83",
                "terminal.ansi.green": "#768BA4",
                "terminal.ansi.yellow": "#9D9CB2",
                "terminal.ansi.blue": "#B1B5C7",
                "terminal.ansi.magenta": "#C2B8C7",
                "terminal.ansi.cyan": "#BCC7D0",
                "terminal.ansi.white": "#c4c6cc",
                "terminal.ansi.bright_black": "#65687b",
                "terminal.ansi.bright_red": "#8D6A83",
                "terminal.ansi.bright_green": "#768BA4",
                "terminal.ansi.bright_yellow": "#9D9CB2",
                "terminal.ansi.bright_blue": "#B1B5C7",
                "terminal.ansi.bright_magenta": "#C2B8C7",
                "terminal.ansi.bright_cyan": "#BCC7D0",
                "terminal.ansi.bright_white": "#c4c6cc",
                "modified": "#8D6A83",
                "syntax": {
                    "attribute": {
                        "color": "#8D6A83"
                    },
                    "boolean": {
                      "color": "#768BA4"
                    },
                    "comment": {
                      "color": "#65687b"
                    },
                    "comment.doc": {
                      "color": "#65687b"
                    },
                    "constant": {
                      "color": "#768BA4"
                    },
                    "function": {
                      "color": "#BCC7D0"
                    },
                    "keyword": {
                      "color": "#B1B5C7"
                    },
                    "number": {
                      "color": "#C2B8C7"
                    },
                    "operator": {
                      "color": "#B1B5C7"
                    },
                    "string": {
                      "color": "#8D6A83"
                    },
                    "variable": {
                      "color": "#768BA4"
                    }
                },
                "players": [
                  {
                    "cursor": "#8D6A83",
                    "background": "#000000",
                    "selection": "#8D6A8330"
                  }
                ]
            }
        }
    ]
}
