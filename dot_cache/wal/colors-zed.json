{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c7cbcb",
                "text": "#c7cbcb",
                "text.muted": "#c7cbcb70",
                "text.ignored": "#c7cbcb40",
                "text.placeholder": "#c7cbcb50",
                "ignored": "#c7cbcb30",
                "element.hover": "#c7cbcb30",
                "ghost_element.hover": "#6DBFC430",
                "ghost_element.selected": "#6DBFC430",
                "ghost_element.active": "#6DBFC460",
                "border": "#000000",
                "editor.highlighted_line.background": "#6DBFC410",
                "editor.active_line.background": "#6DBFC410",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#6DBFC430",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#6DBFC4",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c7cbcb",
                "terminal.dim_foreground": "#c7cbcb",
                "terminal.bright_foreground": "#c7cbcb",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#6DBFC4",
                "terminal.ansi.green": "#9BA5A0",
                "terminal.ansi.yellow": "#DDA8AA",
                "terminal.ansi.blue": "#9BC6BA",
                "terminal.ansi.magenta": "#D6BEC1",
                "terminal.ansi.cyan": "#A4D5CD",
                "terminal.ansi.white": "#c7cbcb",
                "terminal.ansi.bright_black": "#687f7f",
                "terminal.ansi.bright_red": "#6DBFC4",
                "terminal.ansi.bright_green": "#9BA5A0",
                "terminal.ansi.bright_yellow": "#DDA8AA",
                "terminal.ansi.bright_blue": "#9BC6BA",
                "terminal.ansi.bright_magenta": "#D6BEC1",
                "terminal.ansi.bright_cyan": "#A4D5CD",
                "terminal.ansi.bright_white": "#c7cbcb",
                "modified": "#6DBFC4",
                "syntax": {
                    "attribute": {
                        "color": "#6DBFC4"
                    },
                    "boolean": {
                      "color": "#9BA5A0"
                    },
                    "comment": {
                      "color": "#687f7f"
                    },
                    "comment.doc": {
                      "color": "#687f7f"
                    },
                    "constant": {
                      "color": "#9BA5A0"
                    },
                    "function": {
                      "color": "#A4D5CD"
                    },
                    "keyword": {
                      "color": "#9BC6BA"
                    },
                    "number": {
                      "color": "#D6BEC1"
                    },
                    "operator": {
                      "color": "#9BC6BA"
                    },
                    "string": {
                      "color": "#6DBFC4"
                    },
                    "variable": {
                      "color": "#9BA5A0"
                    }
                },
                "players": [
                  {
                    "cursor": "#6DBFC4",
                    "background": "#000000",
                    "selection": "#6DBFC430"
                  }
                ]
            }
        }
    ]
}
