{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c4c2c7",
                "text": "#c4c2c7",
                "text.muted": "#c4c2c770",
                "text.ignored": "#c4c2c740",
                "text.placeholder": "#c4c2c750",
                "ignored": "#c4c2c730",
                "element.hover": "#c4c2c730",
                "ghost_element.hover": "#9C4C6D30",
                "ghost_element.selected": "#9C4C6D30",
                "ghost_element.active": "#9C4C6D60",
                "border": "#000000",
                "editor.highlighted_line.background": "#9C4C6D10",
                "editor.active_line.background": "#9C4C6D10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#9C4C6D30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#9C4C6D",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c4c2c7",
                "terminal.dim_foreground": "#c4c2c7",
                "terminal.bright_foreground": "#c4c2c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#9C4C6D",
                "terminal.ansi.green": "#B8807A",
                "terminal.ansi.yellow": "#59708F",
                "terminal.ansi.blue": "#AA678A",
                "terminal.ansi.magenta": "#E16C90",
                "terminal.ansi.cyan": "#7092AC",
                "terminal.ansi.white": "#c4c2c7",
                "terminal.ansi.bright_black": "#655c71",
                "terminal.ansi.bright_red": "#9C4C6D",
                "terminal.ansi.bright_green": "#B8807A",
                "terminal.ansi.bright_yellow": "#59708F",
                "terminal.ansi.bright_blue": "#AA678A",
                "terminal.ansi.bright_magenta": "#E16C90",
                "terminal.ansi.bright_cyan": "#7092AC",
                "terminal.ansi.bright_white": "#c4c2c7",
                "modified": "#9C4C6D",
                "syntax": {
                    "attribute": {
                        "color": "#9C4C6D"
                    },
                    "boolean": {
                      "color": "#B8807A"
                    },
                    "comment": {
                      "color": "#655c71"
                    },
                    "comment.doc": {
                      "color": "#655c71"
                    },
                    "constant": {
                      "color": "#B8807A"
                    },
                    "function": {
                      "color": "#7092AC"
                    },
                    "keyword": {
                      "color": "#AA678A"
                    },
                    "number": {
                      "color": "#E16C90"
                    },
                    "operator": {
                      "color": "#AA678A"
                    },
                    "string": {
                      "color": "#9C4C6D"
                    },
                    "variable": {
                      "color": "#B8807A"
                    }
                },
                "players": [
                  {
                    "cursor": "#9C4C6D",
                    "background": "#000000",
                    "selection": "#9C4C6D30"
                  }
                ]
            }
        }
    ]
}
