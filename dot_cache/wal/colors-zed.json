{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c5c7c7",
                "text": "#c5c7c7",
                "text.muted": "#c5c7c770",
                "text.ignored": "#c5c7c740",
                "text.placeholder": "#c5c7c750",
                "ignored": "#c5c7c730",
                "element.hover": "#c5c7c730",
                "ghost_element.hover": "#6AC5AD30",
                "ghost_element.selected": "#6AC5AD30",
                "ghost_element.active": "#6AC5AD60",
                "border": "#000000",
                "editor.highlighted_line.background": "#6AC5AD10",
                "editor.active_line.background": "#6AC5AD10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#6AC5AD30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#6AC5AD",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c5c7c7",
                "terminal.dim_foreground": "#c5c7c7",
                "terminal.bright_foreground": "#c5c7c7",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#6AC5AD",
                "terminal.ansi.green": "#95A8A4",
                "terminal.ansi.yellow": "#9ED4A3",
                "terminal.ansi.blue": "#D6D89D",
                "terminal.ansi.magenta": "#94B7CF",
                "terminal.ansi.cyan": "#95CDCD",
                "terminal.ansi.white": "#c5c7c7",
                "terminal.ansi.bright_black": "#607572",
                "terminal.ansi.bright_red": "#6AC5AD",
                "terminal.ansi.bright_green": "#95A8A4",
                "terminal.ansi.bright_yellow": "#9ED4A3",
                "terminal.ansi.bright_blue": "#D6D89D",
                "terminal.ansi.bright_magenta": "#94B7CF",
                "terminal.ansi.bright_cyan": "#95CDCD",
                "terminal.ansi.bright_white": "#c5c7c7",
                "modified": "#6AC5AD",
                "syntax": {
                    "attribute": {
                        "color": "#6AC5AD"
                    },
                    "boolean": {
                      "color": "#95A8A4"
                    },
                    "comment": {
                      "color": "#607572"
                    },
                    "comment.doc": {
                      "color": "#607572"
                    },
                    "constant": {
                      "color": "#95A8A4"
                    },
                    "function": {
                      "color": "#95CDCD"
                    },
                    "keyword": {
                      "color": "#D6D89D"
                    },
                    "number": {
                      "color": "#94B7CF"
                    },
                    "operator": {
                      "color": "#D6D89D"
                    },
                    "string": {
                      "color": "#6AC5AD"
                    },
                    "variable": {
                      "color": "#95A8A4"
                    }
                },
                "players": [
                  {
                    "cursor": "#6AC5AD",
                    "background": "#000000",
                    "selection": "#6AC5AD30"
                  }
                ]
            }
        }
    ]
}
