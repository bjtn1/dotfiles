{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c1c3c4",
                "text": "#c1c3c4",
                "text.muted": "#c1c3c470",
                "text.ignored": "#c1c3c440",
                "text.placeholder": "#c1c3c450",
                "ignored": "#c1c3c430",
                "element.hover": "#c1c3c430",
                "ghost_element.hover": "#C4AB3F30",
                "ghost_element.selected": "#C4AB3F30",
                "ghost_element.active": "#C4AB3F60",
                "border": "#000000",
                "editor.highlighted_line.background": "#C4AB3F10",
                "editor.active_line.background": "#C4AB3F10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#C4AB3F30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#C4AB3F",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c1c3c4",
                "terminal.dim_foreground": "#c1c3c4",
                "terminal.bright_foreground": "#c1c3c4",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#C4AB3F",
                "terminal.ansi.green": "#E0DB0E",
                "terminal.ansi.yellow": "#EFE709",
                "terminal.ansi.blue": "#95834A",
                "terminal.ansi.magenta": "#F59659",
                "terminal.ansi.cyan": "#1D728A",
                "terminal.ansi.white": "#c1c3c4",
                "terminal.ansi.bright_black": "#59626c",
                "terminal.ansi.bright_red": "#C4AB3F",
                "terminal.ansi.bright_green": "#E0DB0E",
                "terminal.ansi.bright_yellow": "#EFE709",
                "terminal.ansi.bright_blue": "#95834A",
                "terminal.ansi.bright_magenta": "#F59659",
                "terminal.ansi.bright_cyan": "#1D728A",
                "terminal.ansi.bright_white": "#c1c3c4",
                "modified": "#C4AB3F",
                "syntax": {
                    "attribute": {
                        "color": "#C4AB3F"
                    },
                    "boolean": {
                      "color": "#E0DB0E"
                    },
                    "comment": {
                      "color": "#59626c"
                    },
                    "comment.doc": {
                      "color": "#59626c"
                    },
                    "constant": {
                      "color": "#E0DB0E"
                    },
                    "function": {
                      "color": "#1D728A"
                    },
                    "keyword": {
                      "color": "#95834A"
                    },
                    "number": {
                      "color": "#F59659"
                    },
                    "operator": {
                      "color": "#95834A"
                    },
                    "string": {
                      "color": "#C4AB3F"
                    },
                    "variable": {
                      "color": "#E0DB0E"
                    }
                },
                "players": [
                  {
                    "cursor": "#C4AB3F",
                    "background": "#000000",
                    "selection": "#C4AB3F30"
                  }
                ]
            }
        }
    ]
}
