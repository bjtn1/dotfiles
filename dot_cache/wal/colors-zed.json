{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c1c1c3",
                "text": "#c1c1c3",
                "text.muted": "#c1c1c370",
                "text.ignored": "#c1c1c340",
                "text.placeholder": "#c1c1c350",
                "ignored": "#c1c1c330",
                "element.hover": "#c1c1c330",
                "ghost_element.hover": "#225C8E30",
                "ghost_element.selected": "#225C8E30",
                "ghost_element.active": "#225C8E60",
                "border": "#000000",
                "editor.highlighted_line.background": "#225C8E10",
                "editor.active_line.background": "#225C8E10",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#225C8E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#225C8E",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c1c1c3",
                "terminal.dim_foreground": "#c1c1c3",
                "terminal.bright_foreground": "#c1c1c3",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#225C8E",
                "terminal.ansi.green": "#1E61AE",
                "terminal.ansi.yellow": "#5B6E90",
                "terminal.ansi.blue": "#54A8C6",
                "terminal.ansi.magenta": "#8E99AF",
                "terminal.ansi.cyan": "#9AB3CC",
                "terminal.ansi.white": "#c1c1c3",
                "terminal.ansi.bright_black": "#575a6a",
                "terminal.ansi.bright_red": "#225C8E",
                "terminal.ansi.bright_green": "#1E61AE",
                "terminal.ansi.bright_yellow": "#5B6E90",
                "terminal.ansi.bright_blue": "#54A8C6",
                "terminal.ansi.bright_magenta": "#8E99AF",
                "terminal.ansi.bright_cyan": "#9AB3CC",
                "terminal.ansi.bright_white": "#c1c1c3",
                "modified": "#225C8E",
                "syntax": {
                    "attribute": {
                        "color": "#225C8E"
                    },
                    "boolean": {
                      "color": "#1E61AE"
                    },
                    "comment": {
                      "color": "#575a6a"
                    },
                    "comment.doc": {
                      "color": "#575a6a"
                    },
                    "constant": {
                      "color": "#1E61AE"
                    },
                    "function": {
                      "color": "#9AB3CC"
                    },
                    "keyword": {
                      "color": "#54A8C6"
                    },
                    "number": {
                      "color": "#8E99AF"
                    },
                    "operator": {
                      "color": "#54A8C6"
                    },
                    "string": {
                      "color": "#225C8E"
                    },
                    "variable": {
                      "color": "#1E61AE"
                    }
                },
                "players": [
                  {
                    "cursor": "#225C8E",
                    "background": "#000000",
                    "selection": "#225C8E30"
                  }
                ]
            }
        }
    ]
}
