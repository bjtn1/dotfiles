{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#00000090",
                "editor.background": "#00000090",
                "editor.foreground": "#c3c4c4",
                "text": "#c3c4c4",
                "text.muted": "#c3c4c470",
                "text.ignored": "#c3c4c440",
                "text.placeholder": "#c3c4c450",
                "ignored": "#c3c4c430",
                "element.hover": "#c3c4c430",
                "ghost_element.hover": "#0C649230",
                "ghost_element.selected": "#0C649230",
                "ghost_element.active": "#0C649260",
                "border": "#000000",
                "editor.highlighted_line.background": "#0C649210",
                "editor.active_line.background": "#0C649210",
                "panel.background": "#00000090",
                "title_bar.background": "#00000090",
                "title_bar.inactive_background": "#00000090",
                "status_bar.background": "#00000090",
                "drop_target.background": "#00000090",
                "elevated_surface.background": "#000000",
                "toolbar.background": "#00000090",
                "tab_bar.background": "#00000090",
                "tab.inactive_background": "#00000090",
                "tab.active_background": "#0C649230",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#000000",
                "scrollbar.thumb.background": "#0C6492",
                "editor.gutter.background": "#00000090",
                "terminal.background": "#00000010",
                "terminal.foreground": "#c3c4c4",
                "terminal.dim_foreground": "#c3c4c4",
                "terminal.bright_foreground": "#c3c4c4",
                "terminal.ansi.black": "#000000",
                "terminal.ansi.red": "#0C6492",
                "terminal.ansi.green": "#0274AD",
                "terminal.ansi.yellow": "#627887",
                "terminal.ansi.blue": "#977C85",
                "terminal.ansi.magenta": "#4C9CB5",
                "terminal.ansi.cyan": "#9A9E9E",
                "terminal.ansi.white": "#c3c4c4",
                "terminal.ansi.bright_black": "#5a6f6f",
                "terminal.ansi.bright_red": "#0C6492",
                "terminal.ansi.bright_green": "#0274AD",
                "terminal.ansi.bright_yellow": "#627887",
                "terminal.ansi.bright_blue": "#977C85",
                "terminal.ansi.bright_magenta": "#4C9CB5",
                "terminal.ansi.bright_cyan": "#9A9E9E",
                "terminal.ansi.bright_white": "#c3c4c4",
                "modified": "#0C6492",
                "syntax": {
                    "attribute": {
                        "color": "#0C6492"
                    },
                    "boolean": {
                      "color": "#0274AD"
                    },
                    "comment": {
                      "color": "#5a6f6f"
                    },
                    "comment.doc": {
                      "color": "#5a6f6f"
                    },
                    "constant": {
                      "color": "#0274AD"
                    },
                    "function": {
                      "color": "#9A9E9E"
                    },
                    "keyword": {
                      "color": "#977C85"
                    },
                    "number": {
                      "color": "#4C9CB5"
                    },
                    "operator": {
                      "color": "#977C85"
                    },
                    "string": {
                      "color": "#0C6492"
                    },
                    "variable": {
                      "color": "#0274AD"
                    }
                },
                "players": [
                  {
                    "cursor": "#0C6492",
                    "background": "#000000",
                    "selection": "#0C649230"
                  }
                ]
            }
        }
    ]
}
